        SYNTAX      INTEGER {
            option60String(1),
            usernameAndPassword(2)
        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "IPoE test authentication mode.
             option60String(1)      - authenticate using
                                      zxAnPonRmIpoeTestOption60String.
             usernameAndPassword(2) - authenticate using 
                                      zxAnPonRmIpoeTestUsername
                                      and zxAnPonRmIpoeTestPassword."
        REFERENCE
            "GPON in ZXPON ZTE private OMCI & PLOAM Specification 3.8.5~3.8.6;
            EPON in ZXPON ZTE private extended 
            OAM Specification 3.3.2.30~3.3.2.31"
        DEFVAL      { option60String }
        ::= { zxAnPonRmOnuIpoeTestEntry 1 }

