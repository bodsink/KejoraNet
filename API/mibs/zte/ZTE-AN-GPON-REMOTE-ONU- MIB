-- Following management objects are defined.

-- zxAnPonRmOnuObjects
        -- zxAnPonRmOnuIfProtoConfTable(not support this  table)
    -- zxAnPonRmOnuVoipObjects   
        -- zxAnPonRmPotsCallDataRecordTable(not support this table)
    -- zxAnPonRmOnuCliOverSnmpObjects    
        -- zxAnPonRmOnuRetrieveCmdTable
    -- zxAnPonRmOnuCfmObjects
        -- zxAnPonRmOnuCfmMdTable
        -- zxAnPonRmOnuCfmMaTable
        -- zxAnPonRmOnuCfmMepTable
    -- zxAnPonRmOnuOamObjects
        -- zxAnPonRmOnuPppoeTestTable
        -- zxAnPonRmOnuIpoeTestTable
        -- zxAnPonRmOnuVoipCalleeTestTable
        -- zxAnPonRmOnuVoipCallerTestTable
        -- zxAnPonRmOnuOTransceiversTable        
--------------------------------------------------------------------------------   
zxAnPonRmOnuObjects         OBJECT IDENTIFIER ::= { zxAnPonRemoteOnuMib 2 }
zxAnPonRmOnuProtocolObjects OBJECT IDENTIFIER ::= { zxAnPonRmOnuObjects 1 }
zxAnPonRmOnuVoipObjects     OBJECT IDENTIFIER ::= { zxAnPonRmOnuObjects 2 }
zxAnPonRmOnuCliOverSnmpObjects      OBJECT IDENTIFIER ::=
                                                    { zxAnPonRmOnuObjects 5 }
zxAnPonRmOnuCfmObjects      OBJECT IDENTIFIER ::= { zxAnPonRmOnuObjects 8 }

zxAnPonRmOnuOamObjects      OBJECT IDENTIFIER ::= { zxAnPonRmOnuObjects 9 }

