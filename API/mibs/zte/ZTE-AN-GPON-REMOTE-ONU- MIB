------------------------------------------------------------------------------
--5.3 zxAnPonRmOnuRetrieveCmdTable
------------------------------------------------------------------------------
    zxAnPonRmOnuRetrieveCmdTable   OBJECT-TYPE
        SYNTAX      SEQUENCE  OF  ZxAnPonRmOnuRetrieveCmdEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "EMS can send zxAnPonRmOnuRetrieveCmd to retrieve ONU monitoring
             metrics which are not defined by ME of OMCI or OAM.
             Otherwise, EMS can retrieve error messages of excution of
             zxAnPonRmOnuConfCmd  in ONU.

            The action is divided into two steps.
            Step one:
                EMS sends zxAnPonRmOnuRetrieveCmdSessionId and
                zxAnPonRmOnuRetrieveCmd to OLT.
                The format of zxAnPonRmOnuRetrieveCmd is:
                'command head::[parameter 1]=P1,[parameter 2]=P2,
                 ...[parameter n]=Pn;'
                the command head represents a configuration function 
                which is defined by EMS and ONU.
                parameters are followed by '::' and separated by ',',
                the end of the command must be ';'.
                Example:
                 GET_WAN_CONF::WANID=1;                 
            Step two:  
                The result is saved in zxAnPonRmOnuRetrieveCmdReply.
                EMS can retrieve result by zxAnPonRmOnuRetrieveCmdSessionId.
                The format of zxAnPonRmOnuRetrieveCmdReply is:
                'command head::[parameter 1]=P1,[parameter 2]=P2,
                 ...[parameter n]=Pn;'
                the command head represents a configuration function 
                which is defined by EMS and ONU.
                parameters are followed by '::' and separated by ',',
                the end of the command must be ';'.
                Example:
                 GET_WAN_CONF_RESULT::WANID=1,HOST=1,MODE=1,PPPUser=test,
                   PPPPassword=test;  "
        ::=  { zxAnPonRmOnuCliOverSnmpObjects  3 }

    zxAnPonRmOnuRetrieveCmdEntry   OBJECT-TYPE
        SYNTAX      ZxAnPonRmOnuRetrieveCmdEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "An entry in zxAnPonRmOnuRetrieveCmdTable."
        INDEX { ifIndex,zxAnPonOnuIndex,
                zxAnPonRmOnuRetrieveCmdSessionId }
        ::=  { zxAnPonRmOnuRetrieveCmdTable 1 }

    ZxAnPonRmOnuRetrieveCmdEntry  ::=  SEQUENCE {
        zxAnPonRmOnuRetrieveCmdSessionId    Integer32,
        zxAnPonRmOnuRetrieveCmd             OCTET STRING,
        zxAnPonRmOnuRetrieveCmdReply        OCTET STRING
        }


    zxAnPonRmOnuRetrieveCmdSessionId OBJECT-TYPE
        SYNTAX      Integer32(0..65535)
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This attribute identifies the session ID.
             The session ID is generated by EMS and is unique in EMS.
             Session ID can ensure that reply result does not conflicting."
        ::= { zxAnPonRmOnuRetrieveCmdEntry 1 }

    zxAnPonRmOnuRetrieveCmd    OBJECT-TYPE
        SYNTAX      OCTET STRING(SIZE(0..1024))
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "This attribute identifies commands
            which declared the information which EMS needs.
            "
        ::= { zxAnPonRmOnuRetrieveCmdEntry 2 }

    zxAnPonRmOnuRetrieveCmdReply    OBJECT-TYPE
        SYNTAX      OCTET STRING(SIZE(0..1024))
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "This attribute identifies the reply result.
            "
        ::= { zxAnPonRmOnuRetrieveCmdEntry 3 }

-------------------------------------------------------------------------------
--8 zxAnPonRmOnuCfmObjects
-------------------------------------------------------------------------------
--8.2 zxAnPonRmOnuCfmMdTable
-------------------------------------------------------------------------------
    zxAnPonRmOnuCfmMdTable OBJECT-TYPE
        SYNTAX SEQUENCE OF ZxAnPonRmOnuCfmMdEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "The Maintenance Domain table. Each row in the table
             represents a different Maintenance Domain.
             A Maintenance Domain is described in 802.1ag (3.22) as the
             network or the part of the network for which faults in
             connectivity are to be managed. The boundary of a Maintenance
             Domain is defined by a set of DSAPs, each of which can become
             a point of connectivity to a service instance.
             This table only used by GPON service.
            "
        REFERENCE
        "ITU-T G988 clause 9.3.19"
        ::= { zxAnPonRmOnuCfmObjects 2 }

    zxAnPonRmOnuCfmMdEntry OBJECT-TYPE
        SYNTAX ZxAnPonRmOnuCfmMdEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "The Maintenance Domain table entry.
            The index 'zxAnPonOnuIndex' represents the normal ONU index.
            "
       INDEX {ifIndex, zxAnPonOnuIndex, zxAnPonRmOnuCfmMdId }
       ::= { zxAnPonRmOnuCfmMdTable 1 }

    ZxAnPonRmOnuCfmMdEntry ::= SEQUENCE {
            zxAnPonRmOnuCfmMdId                 Integer32,
            zxAnPonRmOnuCfmMdFormat             INTEGER,
            zxAnPonRmOnuCfmMdName               OCTET STRING,
            zxAnPonRmOnuCfmMdMdLevel            Integer32,
            zxAnPonRmOnuCfmMdMhfCreation        INTEGER,
            zxAnPonRmOnuCfmSendIdPermission     INTEGER,
            zxAnPonRmOnuCfmMdRowStatus          RowStatus
            }

    zxAnPonRmOnuCfmMdId OBJECT-TYPE
        SYNTAX Integer32 (1..16)
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
             "The index to the Maintenance Domain table.
              Referential integrity is required, i.e., the index needs to be
              persistent upon a reboot or restart of a device. The index
              can never be reused for other Maintenance Domain. The index
              value should keep increasing up to the time that they wrap
              around. This is to facilitate access control based on OID.
            "
        REFERENCE
        "ITU-T G988 clause 9.3.19"
       ::= { zxAnPonRmOnuCfmMdEntry 1 }

    zxAnPonRmOnuCfmMdFormat OBJECT-TYPE
        SYNTAX INTEGER {
            none              (1),
            dnsLikeName       (2),
            macAddressAndUint (3),
            charString        (4),
            iccBased          (5)
        }
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
            "The type (and thereby format) of the Maintenance Domain Name.
             A value that represents a type (and thereby the format)
             of a zxAnPonRmOnuCfmMdName.  The value can be one of
             the following:
             none(1)           No format specified, usually because
                               there is not (yet) a Maintenance
                               Domain Name. In this case, a zero
                               length OCTET STRING for the Domain
                               Name field is acceptable.
             dnsLikeName(2)    Domain Name like string, globally unique
                               text string derived from a DNS name.
             macAddrAndUint(3) MAC address + 2-octet (unsigned) integer.
             charString(4)     RFC2579 DisplayString, except that the
                               character codes 0-31 (decimal) are not
                               used.
             iccBased(5)       ITU carrier code followed by locally
                               assigned UMC code, 13 bytes with trailing
                               nulls as needed.
             "
        REFERENCE
         "802.1ag clause 21.6.5.1; ITU-T Y.1731; ITU-T G988 9.3.19"
        DEFVAL { charString }
        ::= { zxAnPonRmOnuCfmMdEntry 2 }

    zxAnPonRmOnuCfmMdName OBJECT-TYPE
        SYNTAX  OCTET STRING(SIZE(0..45))
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
            "The Maintenance Domain name. The type/format of this object
            is determined by the value of the zxAnPonRmOnuCfmMdFormat object.
            Each Maintenance Domain has unique name amongst all those
            used or available to a service provider or operator. It
            facilitates easy identification of administrative
            responsibility for each Maintenance Domain.
            Clause 3.24 defines a Maintenance Domain name as the
            identifier, unique over the domain for which CFM is to
            protect against accidental concatenation of Service
            Instances, of a particular Maintenance Domain.
            "
        REFERENCE
         "802.1ag clause 21.6.5.1; ITU-T Y.1731; ITU-T G988 9.3.19"
        ::= { zxAnPonRmOnuCfmMdEntry 3 }

    zxAnPonRmOnuCfmMdMdLevel OBJECT-TYPE
        SYNTAX Integer32 (0..7)
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
             "The Maintenance Domain Level.
             Integer identifying the Maintenance Domain Level (MD Level).
             Higher numbers correspond to higher Maintenance Domains,
             those with the greatest physical reach, with the highest
             values for customer's CFM packets. Lower numbers correspond
             to lower Maintenance Domains, those with more limited
             physical reach, with the lowest values for CFM PDUs
             protecting single bridges or physical links."
        REFERENCE
         "802.1ag clause 21.6.5.1; ITU-T Y.1731; ITU-T G988 9.3.19"
        DEFVAL { 0 }
        ::= { zxAnPonRmOnuCfmMdEntry 4 }

    zxAnPonRmOnuCfmMdMhfCreation OBJECT-TYPE
        SYNTAX INTEGER {
            defMHFnone     (1),
            defMHFdefault  (2),
            defMHFexplicit (3)
        }
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
            "Indicates if the Management Entity can create MHFs.
            Enumerated value indicating whether the management entity can
            create MHFs (MIP Half Function) for this Maintenance Domain.

            The valid values are:

            defMHFnone(1)      No MHFs can be created for this VID.
            defMHFdefault(2)   MHFs can be created on this VID on any
                               Bridge port through which this VID can
                               pass.
            defMHFexplicit(3)  MHFs can be created for this VID only on
                               Bridge ports through which this VID can
                               pass, and only if a MEP is created at some
                               lower MD Level.
            "
        REFERENCE
            "802.1ag clause 12.14.5.1.3:c"
        DEFVAL { defMHFnone }
        ::= { zxAnPonRmOnuCfmMdEntry 5 }

    zxAnPonRmOnuCfmSendIdPermission OBJECT-TYPE
        SYNTAX INTEGER {
             sendIdNone          (1),
             sendIdChassis       (2),
             sendIdManage        (3),
             sendIdChassisManage (4)
        }
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
             "Enumerated value indicating what, if anything, is to be
             included in the Sender ID TLV (21.5.3) transmitted by MPs
             configured in this Maintenance Domain. Since, in this
             variable, there is no encompassing Maintenance Domain, the
             value sendIdDefer takes the meaning of sendIdChassisManage.
             The valid values are:

             sendIdNone(1)      The Sender ID TLV is not to be sent.
             sendIdChassis(2)   The Chassis ID Length, Chassis ID
                                Subtype, and Chassis ID fields of  the
                                Sender ID TLV are to be sent.
             sendIdManage(3)    The Management Address Length and
                                Management Address of the Sender ID TLV
                                are to be sent.
             sendIdChassisManage(4) The Chassis ID Length, Chassis ID
                                Subtype, Chassis ID, Management Address
                                Length and Management Address fields are
                                all to be sent.
             "
        REFERENCE
             "802.1ag clause 12.14.5.1.3:d"
        DEFVAL { sendIdNone }
        ::= { zxAnPonRmOnuCfmMdEntry 6 }

    zxAnPonRmOnuCfmMdRowStatus OBJECT-TYPE
        SYNTAX RowStatus
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
             "The status of the row.
             The writable columns in a row can not be changed if the row
             is active. All columns must have a valid value before a row
             can be activated.
             "
        ::= { zxAnPonRmOnuCfmMdEntry 50 }

-------------------------------------------------------------------------------
--8.3 zxAnPonRmOnuCfmMaTable
-------------------------------------------------------------------------------

    zxAnPonRmOnuCfmMaTable    OBJECT-TYPE
        SYNTAX      SEQUENCE OF ZxAnPonRmOnuCfmMaEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This managed entity models an IEEE 802.1 service defined
             on a bridge port. A maintenance association (MA) is
             a set of endpoints on opposite sides of a network,
             all existing at a defined maintenance level.
             This table only used by GPON service."
        REFERENCE   "G.984.4 9.3.20 Dot1ag maintenance association"
        ::= { zxAnPonRmOnuCfmObjects 3 }

    zxAnPonRmOnuCfmMaEntry OBJECT-TYPE
        SYNTAX      ZxAnPonRmOnuCfmMaEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "It is entry in the zxAnPonRmOnuCfmMaTable."
        INDEX   {ifIndex, zxAnPonOnuIndex, zxAnPonRmOnuCfmMdId, 
                 zxAnPonOnuCfmMaId }
        ::= { zxAnPonRmOnuCfmMaTable 1 }

    ZxAnPonRmOnuCfmMaEntry ::=
        SEQUENCE {
            zxAnPonOnuCfmMaId                  Integer32,
            zxAnPonOnuCfmMaNameFormat          INTEGER,
            zxAnPonOnuCfmMaName                OCTET STRING,
            zxAnPonOnuCfmMaCcmInterval         INTEGER,
            zxAnPonOnuCfmMaVlans               DisplayString,
            zxAnPonOnuCfmMaMhfCreation         INTEGER,
            zxAnPonOnuCfmMaIdPermission        INTEGER,
            zxAnPonOnuCfmMaRowStatus           RowStatus
        }

    zxAnPonOnuCfmMaId OBJECT-TYPE
        SYNTAX      Integer32 (1..16)
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This attribute uniquely identifies an instance of MA."
        ::= { zxAnPonRmOnuCfmMaEntry 1 }

    zxAnPonOnuCfmMaNameFormat OBJECT-TYPE
        SYNTAX      INTEGER {
            primaryVid        (1),
            charString        (2),
            unsignedInt16     (3),
            rfc2865VpnId      (4)
        }
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "A value that represents a type (and thereby the format)
        of a zxAnPonOnuCfmMaName.  The value can be one of
        the following:

        primaryVid(1)     Primary VLAN ID.
                          12 bits represented in a 2-octet integer:
                          - 4 least significant bits of the first
                            byte contains the 4 most significant
                            bits of the 12 bits primary VID
                          - second byte contains the 8 least
                            significant bits of the primary VID

                                 0 1 2 3 4 5 6 7 8
                                 +-+-+-+-+-+-+-+-+
                                 |0 0 0 0| (MSB) |
                                 +-+-+-+-+-+-+-+-+
                                 |  VID   LSB    |
                                 +-+-+-+-+-+-+-+-+

        charString(2)     RFC2579 DisplayString, except that the
                          character codes 0-31 (decimal) are not
                          used. (1..45) octets
        unsignedInt16 (3) 2-octet integer/big endian
        rfc2865VpnId(4)   RFC 2685 VPN ID
                          3 octet VPN authority Organizationally
                          Unique Identifier followed by 4 octet VPN
                          index identifying VPN according to the OUI:

                                 0 1 2 3 4 5 6 7 8
                                 +-+-+-+-+-+-+-+-+
                                 | VPN OUI (MSB) |
                                 +-+-+-+-+-+-+-+-+
                                 |   VPN OUI     |
                                 +-+-+-+-+-+-+-+-+
                                 | VPN OUI (LSB) |
                                 +-+-+-+-+-+-+-+-+
                                 |VPN Index (MSB)|
                                 +-+-+-+-+-+-+-+-+
                                 |  VPN Index    |
                                 +-+-+-+-+-+-+-+-+
                                 |  VPN Index    |
                                 +-+-+-+-+-+-+-+-+
                                 |VPN Index (LSB)|
                                 +-+-+-+-+-+-+-+-+

        To support future extensions, the zxAnPonOnuCfmMaFormat
        textual convention SHOULD NOT be sub-typed in object type
        definitions.  It MAY be sub-typed in compliance statements in
        order to require only a subset of these address types for a
        compliant implementation.

        Implementations MUST ensure that zxAnPonOnuCfmMaFormat
        objects and any dependent objects (e.g.,
        zxAnPonOnuCfmMaName objects) are consistent.  An
        inconsistentValue error MUST be generated if an attempt to
        change an zxAnPonOnuCfmMaFormat object would, for
        example, lead to an undefined zxAnPonOnuCfmMaName value.
        In particular,
        zxAnPonOnuCfmMaFormat/zxAnPonOnuCfmMaName pairs
        MUST be changed together if the nameType changes.

        The Maintenance Domain name and Maintenance Association name,
        when put together into the CCM PDU, MUST total 48 octets or
        less.  If the zxAnPonRmOnuCfmMdFormat object contains
        none(1), then the zxAnPonOnuCfmMaName object MUST be
        45 octets or less in length.  Otherwise, the length of
        the zxAnPonRmOnuCfmMdName object plus the length of the
        zxAnPonOnuCfmMaName object, added together, MUST total
        less than or equal to 44 octets."
        DEFVAL { charString }
        ::= { zxAnPonRmOnuCfmMaEntry 2 }

    zxAnPonOnuCfmMaName OBJECT-TYPE
        SYNTAX      OCTET STRING(SIZE(0..45))
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "Denotes a generic Maintenance Association Name. It is the
             part of the Maintenance Association Identifier which is
             unique within the Maintenance Domain Name and is appended
             to the Maintenance Domain Name to form the Maintenance
             Association Identifier (MAID).

             The value is always interpreted within
             the context of a zxAnPonOnuCfmMaFormat value.

             The value of a zxAnPonOnuCfmMaName object MUST
             always be consistent with the value of the associated
             zxAnPonOnuCfmMaFormat object. Attempts to set
             an zxAnPonOnuCfmMaName object to a value inconsistent
             with the associated zxAnPonOnuCfmMaFormat MUST fail
             with an inconsistentValue error."
        ::= { zxAnPonRmOnuCfmMaEntry 3 }

    zxAnPonOnuCfmMaCcmInterval OBJECT-TYPE
        SYNTAX      INTEGER {
             intervalInvalid   (1),
             interval300Hz     (2),
             interval10ms      (3),
             interval100ms     (4),
             interval1s        (5),
             interval10s       (6),
             interval1min      (7),
             interval10min     (8)
        }
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "Indicates the interval at which CCMs are sent by a MEP.
             The possible values are:
             intervalInvalid(1)        No CCMs are sent (disabled).
             interval300Hz(2)   CCMs are sent every 3 1/3 milliseconds
                                (300Hz).
             interval10ms(3)    CCMs are sent every 10 milliseconds.
             interval100ms(4)   CCMs are sent every 100 milliseconds.
             interval1s(5)      CCMs are sent every 1 second.
             interval10s(6)     CCMs are sent every 10 seconds.
             interval1min(7)    CCMs are sent every minute.
             interval10min(8)   CCMs are sent every 10 minutes."
        DEFVAL { interval1s }
        ::= { zxAnPonRmOnuCfmMaEntry 4 }

    zxAnPonOnuCfmMaVlans OBJECT-TYPE
        SYNTAX      DisplayString(SIZE(0..64))
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "This attribute is a list of up to 12 VLAN IDs with which
             this MA is associated.
             The first entry is understood to be the primary VLAN.
             eg: 100,15-16 means that the primary VLAN is 100, MA is associated
             with VLAN 15,16,100.
             A null value indicates the MA is not associated with any VLANs."
        ::= { zxAnPonRmOnuCfmMaEntry 5 }

    zxAnPonOnuCfmMaMhfCreation OBJECT-TYPE
        SYNTAX      INTEGER {
            defMHFnone(1),
            defMHFdefault(2),
            defMHFexplicit(3),
            defMHFdefer(4)
        }
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "This attribute determines whether the bridge creates a MHF or not,
             under circumstances defined in IEEE 802.1ag clause 22.2.3.
             defMHFnone(1), No MHFs can be created for this VID.
             defMHFdefault(2),  MHFs can be created on this VID on any
                                Bridge port through which this VID can pass.
             defMHFexplicit(3), MHFs can be created for this VID only on
                                Bridge ports through which this VID can
                                pass, and only if a MEP is created at some
                                lower MD Level.
             defMHFdefer(4),    The creation of MHFs is determined by the
                                corresponding Maintenance Domain variable
                                (zxAnPonRmOnuCfmMdMhfCreation)."
        DEFVAL { defMHFdefer }
        ::= { zxAnPonRmOnuCfmMaEntry 6 }

    zxAnPonOnuCfmMaIdPermission OBJECT-TYPE
        SYNTAX      INTEGER {
            sendIdNone(1),
            sendIdChassis(2),
            sendIdManage(3),
            sendIdChassisManage(4),
            sendIdDefer(5)
        }
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "This attribute determines the content of the sender ID TLV included
             in CFM messages transmitted by MP controlled by this MA.
             sendIdNone(1),     The Sender ID TLV is not to be sent.
             sendIdChassis(2),  The Chassis ID Length, Chassis ID
                                Subtype, and Chassis ID fields of  the
                                Sender ID TLV are to be sent.
             sendIdManage(3),    The Management Address Length and
                                Management Address of the Sender ID TLV
                                are to be sent.
             sendIdChassisManage(4), The Chassis ID Length, Chassis ID
                                Subtype, Chassis ID, Management Address
                                Length and Management Address fields are
                                all to be sent.
             sendIdDefer(5),    The contents of the Sender ID TLV are
                                determined by the corresponding
                                Maintenance Domain variable
                                (zxAnPonRmOnuCfmMdMhfIdPermission)."
        DEFVAL { sendIdDefer }
        ::= { zxAnPonRmOnuCfmMaEntry 7 }

    zxAnPonOnuCfmMaRowStatus OBJECT-TYPE
        SYNTAX      RowStatus
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "This attribute indicates the operation of the entry."
        ::= { zxAnPonRmOnuCfmMaEntry 50 }

-------------------------------------------------------------------------------
--8.4 zxAnPonRmOnuCfmMepTable
-------------------------------------------------------------------------------
    zxAnPonRmOnuCfmMepTable    OBJECT-TYPE
        SYNTAX      SEQUENCE OF ZxAnPonRmOnuCfmMepEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This managed entity models a maintenance association end point
             (MEP) as defined primarily in IEEE 802.1ag 
              and secondarily in ITU-T Y.1731.
             This table only used by GPON service."
        REFERENCE   "G.984.4 9.3.22 Dot1ag MEP"
        ::= { zxAnPonRmOnuCfmObjects 4 }

    zxAnPonRmOnuCfmMepEntry OBJECT-TYPE
        SYNTAX      ZxAnPonRmOnuCfmMepEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "It is entry in the zxAnPonRmOnuCfmMepTable."
        INDEX   { ifIndex, zxAnPonOnuIndex, 
                 zxAnPonRmOnuCfmMdId, zxAnPonOnuCfmMaId, zxAnPonOnuCfmMepId }
        ::= { zxAnPonRmOnuCfmMepTable 1 }

    ZxAnPonRmOnuCfmMepEntry ::=
        SEQUENCE {
            zxAnPonOnuCfmMepId                      Integer32,
            zxAnPonOnuCfmMepEthUniId                ZxAnPonRmOnuUniIndex,
            zxAnPonOnuCfmMepCcmEnabled              INTEGER,
            zxAnPonOnuCfmMepServerEnable            INTEGER,
            zxAnPonOnuCfmMepDirection               INTEGER,
            zxAnPonOnuCfmMepPrimaryVid              Integer32,
            zxAnPonOnuCfmMepAdminState              INTEGER,
            zxAnPonOnuCfmMepPri                     Integer32,
            zxAnPonOnuCfmMepEgressId                OCTET STRING,
            zxAnPonOnuCfmMepRmepIds                 DisplayString,
            zxAnPonOnuCfmMepAisEnable               INTEGER,
            zxAnPonOnuCfmMepAisPeriod               INTEGER,
            zxAnPonOnuCfmMepAisPriority             Integer32,
            zxAnPonOnuCfmMepAisLevel                Integer32,
            zxAnPonOnuCfmMepAlarmThreshold          INTEGER,
            zxAnPonOnuCfmMepAlarmDeSoakTime         Integer32,
            zxAnPonOnuCfmMepAlarmCSoakTime          Integer32,
            zxAnPonOnuCfmMepRowStatus               RowStatus
            }

    zxAnPonOnuCfmMepId OBJECT-TYPE
        SYNTAX      Integer32(1..8191)
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This attribute specifies the MEP's own identity in the MA."
        ::= { zxAnPonRmOnuCfmMepEntry 1 }

    zxAnPonOnuCfmMepEthUniId OBJECT-TYPE
        SYNTAX      ZxAnPonRmOnuUniIndex
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
           "This attribute specifies the associated Ethernet port."
       REFERENCE
           "ITU G988. clause 9.3.22"
        ::= { zxAnPonRmOnuCfmMepEntry 2 }

    zxAnPonOnuCfmMepCcmEnabled OBJECT-TYPE
        SYNTAX      INTEGER {
            enabled  (1),
            disabled (2)
        }
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
           "If set to true, the MEP will generate CCM messages."
        REFERENCE
           "802.1ag clauses 12.14.7.1.3:g and 20.10.1"
        DEFVAL { disabled }
        ::= { zxAnPonRmOnuCfmMepEntry 3 }

    zxAnPonOnuCfmMepServerEnable OBJECT-TYPE
        SYNTAX      INTEGER {
            enabled  (1),
            disabled (2)
        }
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "This attribute specifies that wether to
             enable Y.1731 server MEP function."
       REFERENCE
           "ITU G988. clause 9.3.22"
        DEFVAL { disabled }
        ::= { zxAnPonRmOnuCfmMepEntry 4 }

    zxAnPonOnuCfmMepDirection OBJECT-TYPE
        SYNTAX      INTEGER{
            down(1),
            up(2)
        }
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
           "The direction in which the MEP faces on the Bridge port.
           Indicates the direction in which the Maintenance
           association (MEP or MIP) faces on the bridge port:

           down(1)    Sends Continuity Check Messages away from the
                      MAC Relay Entity.
           up(2)      Sends Continuity Check Messages towards the
                      MAC Relay Entity."
       REFERENCE
           "ITU G988. clause 9.3.22"
        DEFVAL { down }
        ::= { zxAnPonRmOnuCfmMepEntry 5}

    zxAnPonOnuCfmMepPrimaryVid OBJECT-TYPE
        SYNTAX      Integer32  (0..4094)
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
           "An integer indicating the Primary VID of the MEP, always
            one of the VIDs assigned to the MEP's MA.  The value 0
            indicates that either the Primary VID is that of the
            MEP's MA, or that the MEP's MA is associated with no VID."
       REFERENCE
           "ITU G988. clause 9.3.22"
       DEFVAL { 0 }
        ::= { zxAnPonRmOnuCfmMepEntry 6 }

    zxAnPonOnuCfmMepAdminState OBJECT-TYPE
        SYNTAX      INTEGER{
            unlock(1),
            lock(2)
         }
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
           "Administrative state of the MEP,
            unlock indicates that the MEP is to function normally, and
            lock indicates that it is to cease functioning."
       REFERENCE
           "ITU G988. clause 9.3.22"
       DEFVAL { lock }
        ::= { zxAnPonRmOnuCfmMepEntry 7 }

    zxAnPonOnuCfmMepPri OBJECT-TYPE
        SYNTAX      Integer32 (0..7)
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
           "The priority value for CCMs and LTMs transmitted by the MEP.
            Default Value is the highest priority value allowed to pass
            through the bridge port for any of this MEPs VIDs.
            The management entity can obtain the default value for this
            variable from the priority regeneration table by extracting the
            highest priority value in this table on this MEPs bridge port.
            (1 is lowest, then 2, then 0, then 3-7).
            "
       REFERENCE
           "ITU G988. clause 9.3.22"
       DEFVAL { 7 }
        ::= { zxAnPonRmOnuCfmMepEntry 8 }

    zxAnPonOnuCfmMepEgressId OBJECT-TYPE
        SYNTAX      OCTET STRING(SIZE(8))
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "This attribute comprises 8 bytes to be included in LTMs.
             They allow received LTRs to be directed to the correct originator.
             The attribute includes the originator MAC address and a
             locally-defined identifier. If this field is 0 (default),
             the ONT uses the MEP's MAC address, with 0 as the locally-defined
             identifier."
       REFERENCE
           "ITU G988. clause 9.3.22"
        ::= { zxAnPonRmOnuCfmMepEntry 9 }

    zxAnPonOnuCfmMepRmepIds OBJECT-TYPE
        SYNTAX      DisplayString(SIZE(0..64))
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "This attribute lists up to 12 expected peer MEPs for CCMs,
             [IEEE 802.1ag] allows for multipoint networks, and therefore a list 
              of peer MEPs. This attribute allows for up to 12 peers for a 
              given MEP, though G-PON applications are expected to need only 
              a single peer. Missing or unexpected messages trigger alarm 
              declaration after a soak interval. 
              it should be set to null if unused peer MEP.
              e.g., 1,3-5 means the used peer MEPs are 1,3,4,5."
       REFERENCE
           "ITU G988. clause 9.3.22"
        ::= { zxAnPonRmOnuCfmMepEntry 10 }

    zxAnPonOnuCfmMepAisEnable OBJECT-TYPE
        SYNTAX      INTEGER {
            enabled  (1),
            disabled (2)
        }
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "This attribute enable generation of Ethernet AIS."
        DEFVAL { disabled }
        ::= { zxAnPonRmOnuCfmMepEntry 11 }

    zxAnPonOnuCfmMepAisPeriod OBJECT-TYPE
        SYNTAX      INTEGER {
            time1s(1),
            time1min(2)
        }
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "This attribute specifies ethernet AIS transmission period."
        DEFVAL { time1s }
        ::= { zxAnPonRmOnuCfmMepEntry 12 }

    zxAnPonOnuCfmMepAisPriority OBJECT-TYPE
        SYNTAX      Integer32(0..7)
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "This attribute specifies priority of transmitted ETH AIS frames."
        DEFVAL { 7 }
        ::= { zxAnPonRmOnuCfmMepEntry 13 }

    zxAnPonOnuCfmMepAisLevel OBJECT-TYPE
        SYNTAX      Integer32(0..7)
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "This attribute specifies the maintenance
             level at which the client MEP exists."
        DEFVAL { 0 }
        ::= { zxAnPonRmOnuCfmMepEntry 14 }

    zxAnPonOnuCfmMepAlarmThreshold OBJECT-TYPE
        SYNTAX      INTEGER {
            mode1(1),
            mode2(2),
            mode3(3),
            mode4(4),
            mode5(5),
            mode6(6)
        }
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "This attribute specifies the lowest priority alarm
             that is allowed to generate a fault alarm.
             mode1, All defects generate alarms after suitable soaking,
                    including AIS and RDICCM.
             mode2, Alarm generated only by one of: MACstatus, RemoteCCM,
                    ErrorCCM, XconCCM.
             mode3, Alarm generated only by one of: RemoteCCM, ErrorCCM, XconCCM
             mode4, Alarm generated only by one of: ErrorCCM, XconCCM.
             mode5, Alarm generated only by: XconCCM.
             mode6, No alarms are to be reported."
        DEFVAL { mode2 }
        ::= { zxAnPonRmOnuCfmMepEntry 15 }

    zxAnPonOnuCfmMepAlarmDeSoakTime OBJECT-TYPE
        SYNTAX      Integer32(250..1000)
        UNITS       "10ms"
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "This attribute defines the defect soak time
             that must elapse before the MEP declares an alarm."
        DEFVAL { 250 }
        ::= { zxAnPonRmOnuCfmMepEntry 16 }

    zxAnPonOnuCfmMepAlarmCSoakTime OBJECT-TYPE
        SYNTAX      Integer32(250..1000)
        UNITS       "10ms"
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "This attribute defines the defect-free soak time
             that must elapse before the MEP clears an alarm."
        DEFVAL { 1000 }
        ::= { zxAnPonRmOnuCfmMepEntry 17 }

    zxAnPonOnuCfmMepRowStatus OBJECT-TYPE
        SYNTAX      RowStatus
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "This attribute indicates the operation of the entry."
        ::= { zxAnPonRmOnuCfmMepEntry 50 }


--------------------------------------------------------------------------------
--   zxAnPonRmOnuPppoeTestTable
--------------------------------------------------------------------------------

    zxAnPonRmOnuPppoeTestTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF ZxAnPonRmOnuPppoeTestEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The table lists PPPoE test entry. the entry indicates the test."
        REFERENCE
            "GPON in ZXPON ZTE private OMCI & PLOAM Specification 3.8.4;
            EPON in ZXPON ZTE private extended OAM Specification 3.3.5"
        ::= { zxAnPonRmOnuOamObjects 2 }

    zxAnPonRmOnuPppoeTestEntry OBJECT-TYPE
        SYNTAX      ZxAnPonRmOnuPppoeTestEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "An entry in the zxAnPonRmOnuPppoeTestTable."
        INDEX       { ifIndex, zxAnPonOnuIndex, zxAnPonRmOnuIfIndex}
        ::= { zxAnPonRmOnuPppoeTestTable 1 }

    ZxAnPonRmOnuPppoeTestEntry ::= SEQUENCE {
        zxAnPonRmPppoeTestVid          Integer32,
        zxAnPonRmPppoeTestUsername     DisplayString,
        zxAnPonRmPppoeTestPassword     DisplayString,
        zxAnPonRmPppoeTestAuthMode     INTEGER,
        zxAnPonRmPppoeTestAction       INTEGER,
        zxAnPonRmPppoeTestStatus       INTEGER,
        zxAnPonRmPppoeTestFailReason   INTEGER
    }

    zxAnPonRmPppoeTestVid OBJECT-TYPE
        SYNTAX      Integer32 (0..4094)
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The VLAN of interface which will be used in testing datagram.

            0 means that test will use the PVID of interface.
            Non-zero value must be within the tagged VLAN list of interface. If
            ONU can not find the VLAN in the tagged VLAN list of interface, test
            will fail."
        DEFVAL { 0 }
        ::= { zxAnPonRmOnuPppoeTestEntry 1 }

    zxAnPonRmPppoeTestUsername  OBJECT-TYPE
        SYNTAX      DisplayString (SIZE(1..64))
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The user name.max size is 64 for epon, 25 for gpon"
        ::= { zxAnPonRmOnuPppoeTestEntry 2 }

    zxAnPonRmPppoeTestPassword  OBJECT-TYPE
        SYNTAX      DisplayString (SIZE(1..32))
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The password.max size is 32 for EPON, 25 for GPON"
        ::= { zxAnPonRmOnuPppoeTestEntry 3 }

    zxAnPonRmPppoeTestAuthMode  OBJECT-TYPE
        SYNTAX      INTEGER {
            auto(1),
            chap(2),
            pap(3)
        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The protocol that is used to anthenticate user."
        DEFVAL { auto }
        ::= { zxAnPonRmOnuPppoeTestEntry 4 }

    zxAnPonRmPppoeTestAction OBJECT-TYPE
        SYNTAX      INTEGER {
            start(1),
            stop(2)
        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The object is used to start or stop test.
             start(1) - Start the test.
             stop(2)  - Stop the test."
        ::= { zxAnPonRmOnuPppoeTestEntry 5 }

    zxAnPonRmPppoeTestStatus OBJECT-TYPE
        SYNTAX      INTEGER {
            notStarted(1),
            inProgress(2),
            success(3),
            failed(4)
        }
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The status of test. Agent need keep this parameter updated so that
            manager can get the latest status of test by polling.

            notStarted(1) - test is not started
            inProgress(2) - test is in progress
            success(3) - test is completed successfully
            failed(4) - test failed."
        ::= { zxAnPonRmOnuPppoeTestEntry 6 }

    zxAnPonRmPppoeTestFailReason OBJECT-TYPE
        SYNTAX      INTEGER {
            none(1),
            unknownError(2),
            hardwareNotSupport(3),
            vlanNotExist(4),
            padiTimeOut(5),
            padrTimeOut(6),
            lcpNegotiationFail(7),
            userAuthFail(8),
            ipNegotiationFail(9)
        }
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The failed reason of test.

            none(1)               - no error
            unknownError(2)       - unknown error
            hardwareNotSupport(3) - Hardware does not support the test.
            notFindVlan(4)        - Specified VLAN is not existed.
            padiTimeOut(5)        - PADI request timed out.
            padrTimeOut(6)        - PADR request timed out.
            lcpNegotiationFail(7) - LCP negotiation failed.
            userAuthFail(8)       - User name or password is invalid.
            ipNegotiationFail(9)  - IP negotiation failed."
        ::= { zxAnPonRmOnuPppoeTestEntry 7 }


