--  zxAnPonRmOnuVoipCallerTestTable
--------------------------------------------------------------------------------

    zxAnPonRmOnuVoipCallerTestTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF ZxAnPonRmOnuVoipCallerTestEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The table lists caller test entry. The entry indicates a test."
        REFERENCE
            "GPON in ZXPON ZTE private OMCI & PLOAM Specification 3.8.3;
            EPON in ZXPON ZTE private extended OAM Specification 3.3.5"
        ::= { zxAnPonRmOnuOamObjects 6 }

    zxAnPonRmOnuVoipCallerTestEntry OBJECT-TYPE
        SYNTAX      ZxAnPonRmOnuVoipCallerTestEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "An entry in the zxAnPonRmOnuVoipCallerTestTable."
        INDEX       { ifIndex, zxAnPonOnuIndex, zxAnPonRmOnuIfIndex}
        ::= { zxAnPonRmOnuVoipCallerTestTable 1 }

    ZxAnPonRmOnuVoipCallerTestEntry ::= SEQUENCE {
        zxAnPonRmCallerTestTimeout        Integer32,
        zxAnPonRmCallerTestDialedNumber   DisplayString,
        zxAnPonRmCallerTestPortStatus     INTEGER,
        zxAnPonRmCallerTestDelay          Integer32,
        zxAnPonRmCallerTestSipFailReason  INTEGER,
        zxAnPonRmCallerTestSipErrorCode   INTEGER,
        zxAnPonRmCallerTestAction         INTEGER,
        zxAnPonRmCallerTestStatus         INTEGER,
        zxAnPonRmCallerTestFailReason     INTEGER,
        zxAnPonRmCallerTestFailDetail     DisplayString
    }

    zxAnPonRmCallerTestTimeout OBJECT-TYPE
        SYNTAX      Integer32(60..300)
        UNITS       "seconds"
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The time-out of test. Agent should stop test following regular
            on-hook procedure, if the test hasn't completed after time-out yet."
        DEFVAL{ 90 }
        ::= { zxAnPonRmOnuVoipCallerTestEntry 1 }

    zxAnPonRmCallerTestDialedNumber OBJECT-TYPE
        SYNTAX      DisplayString (SIZE (0..64))
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The number that user dials.
            The max size is 64 for EPON ,20 for GPON.
            "
        ::= { zxAnPonRmOnuVoipCallerTestEntry 2 }

    zxAnPonRmCallerTestPortStatus OBJECT-TYPE
        SYNTAX      INTEGER {
            idle(1),
            offHook(2),
            dialTone(3),
            receiveNumber(4),
            receiveEnd(5),
            ringBackTone(6),
            connected(7),
            busyTone(8),
            onHook(9)
        }
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "This object indicates the current status of port.

            idle(1) - The state of port is idle.
            offHook(2) - The state of port is off-hook.
            dialTone(3) - The port is playing dial tone.
            receiveNumber(4) - The port is receiving number.
            receiveEnd(5) - Receiving number ended.
            ringBackTone(6) - The port is playing ring back tone.
            connected(7) - Voice data path is established. The port can receive
            and send voice data.
            busyTone(8) - The port is playing busy tone.
            onHook(9) - The state of port is on-hook.
            "
        ::= { zxAnPonRmOnuVoipCallerTestEntry 3 }

    zxAnPonRmCallerTestDelay OBJECT-TYPE
        SYNTAX      Integer32(0..65535)
        UNITS       "0.1second"
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Specifies how long it takes to set up bearer traffic RTP packets
            for the POTS line being tested in tenths of a second.The delay
            measure timer starts when the INVITE is generated for a call
            origination to the phone number specified by the PHONENO
            parameter in the DGN-SIPDIALTONE TL1 command. The SIP
            application at the ONU starts listening for RTP packets. If RTP
            packets are detected, the delay measure timer is stopped and
            the timer value is recorded for later reporting. Range for measure
            post dial delay is 0.0 to 6553.5 in tenths of seconds."
        ::= { zxAnPonRmOnuVoipCallerTestEntry 4 }

    zxAnPonRmCallerTestSipFailReason  OBJECT-TYPE
        SYNTAX      INTEGER {
            dialToneTestFailure(1),
            inviteOkButNoBearerData(2),
            callTimeoutNo200Ok(3),
            noResponseToInvite(4),
            errorCodeAndReasonPhrase(5),
            callCancelledByServer(6),
            callCancelledByOnt(7),
            lineNotConfigured(8),
            lineNotInValidState(9),
            lineNotRegistered(10),
            notchFiltersRequired(11)
        }
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The failed reason of SIP test. This field should be filled when the
            value of zxAnPonRmCallerTestFailReason is 'sipTestFailed'.

            dialToneTestFailure(1) - Dial tone test failure.
            inviteOkButNoBearerData(2) - No bearer traffic was received even
            though the test call was successful.
            callTimeoutNo200Ok(3) - The test call never completed although
            provisional response were received for the INVITE request.
            noResponseToInvite(4) - No response was received to the INVITE
            request and attempts to retry the INVITE timed out without a
            response.
            errorCodeAndReasonPhrase(5) - The softswitch returned an error to
            the INVITE request.
            callCancelledByServer(6) - The softswitch sent a CANCEL for the
            INVITE transaction.
            callCancelledByOnt(7) - The call was cancelled by the ONT.
            lineNotConfigured(8) - The POTS line is not fully configured,
            probably due to missing SIP provisioning data.
            lineNotInValidState(9)- The POTS line is in the wrong status to
            perform the test.
            lineNotRegistered(10) - The POTS line is not registered with the
            softswitch.
            notchFiltersRequired(11) - The notch filter settings in the
            configuration profile analog data were missing.
            "
        ::= { zxAnPonRmOnuVoipCallerTestEntry 5 }

    zxAnPonRmCallerTestSipErrorCode OBJECT-TYPE
        SYNTAX      INTEGER {
            unauthorized(1),
            forbidden(2),
            notFound(3),
            proxyAuthenticationRequired(4),
            requestTimeout(5),
            temporarilyUnavailable(6),
            addressIncomplete(7),
            busyHere(8),
            requestTerminated(9),
            notAcceptableHere(10),
            serverInternalError(11),
            serviceUnavailable(12),
            serverTimeout(13)
        }
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The error code of SIP test. This field should be filled when the
            value of zxAnPonRmCallerTestSipFailReason is
            'errorCodeAndReasonPhrase'.

            unauthorized(1) - 401 Unauthorized.
            forbidden(2) - 403 Forbidden.
            notFound(3) - 404 Not found.
            proxyAuthenticationRequired(4) - 407 Proxy authentication required.
            requestTimeout(5) - 408 Request timeout.
            temporarilyUnavailable(6) - 480 Temporarily unavailable.
            addressIncomplete(7) - 484 Address incomplete.
            busyHere(8) - 486 Busy here.
            requestTerminated(9) - 487 Request terminated.
            notAcceptableHere(10) - 488 Not acceptable here.
            serverInternalError(11) - 500 Server internal error.
            serviceUnavailable(12) - 503 Service unavailable.
            serverTimeout(13) - 504 Server time-out.
            "
        ::= { zxAnPonRmOnuVoipCallerTestEntry 6 }

    zxAnPonRmCallerTestAction OBJECT-TYPE
        SYNTAX      INTEGER {
            start(1),
            stop(2)
        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The object is used to start or stop test.

            start(1) - Start the test.
            stop(2) - Stop the test. Agent should stop test following regular
            on-hook procedure.
            "
        ::= { zxAnPonRmOnuVoipCallerTestEntry 7 }
        
        
    zxAnPonRmCallerTestStatus OBJECT-TYPE
        SYNTAX      INTEGER {
            notStarted(1),
            inProgress(2),
            success(3),
            failed(4),
            notForced(5)
        }
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The status of test. Agent need keep this parameter updated so that
            manager can get the latest status of test by polling.

            notStarted(1) - Test is not started.
            inProgress(2) - Test is in progress.
            success(3) - Test is successful. It means that the voice path of
            call is well, while the on-hook operation is successful.
            failed(4) - Test failed.
            notForced(5)  - Test is not attempted because the POTS line is busy.
            "
        ::= { zxAnPonRmOnuVoipCallerTestEntry 8 }
        
    zxAnPonRmCallerTestFailReason OBJECT-TYPE
        SYNTAX      INTEGER {
            other(1),
            noDialTone(2),
            noRingBack(3),
            noAnswer(4),
            noUpstreamVoice(5),
            noDownstreamVoice(6),
            noBidirectionVoice(7),
            sipTestFailed(8)
        }
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The failed reason of test.

            other(1) - Other reason.
            noDialTone(2) - No dial tone.
            noRingBack(3) - No ring back tone.
            noAnswer(4) - No answer.
            noUpstreamVoice(5) - No upstream voice.
            noDownstreamVoice(6) - No downstream voice.
            noBidirectionVoice(7) - No upstream and downstream voice.
            sipTestFailed(8) - SIP test failed.
            "
        ::= { zxAnPonRmOnuVoipCallerTestEntry 9 }
        

    zxAnPonRmCallerTestFailDetail OBJECT-TYPE
        SYNTAX      DisplayString (SIZE (0..255))
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The failed detail. This field should be filled when the value of
            zxAnOnuVoipCallerTestFailReason is 'other'."
        ::= { zxAnPonRmOnuVoipCallerTestEntry 10 }

--------------------------------------------------------------------------------
-- zxAnPonRmOnuOTransceiversTable
--------------------------------------------------------------------------------
    zxAnPonRmOnuOTransceiversTable    OBJECT-TYPE
        SYNTAX      SEQUENCE OF ZxAnPonRmOnuOTransceiversEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "ONU optical transceivers table."
        ::= { zxAnPonRmOnuOamObjects 7 }

    zxAnPonRmOnuOTransceiversEntry OBJECT-TYPE
        SYNTAX      ZxAnPonRmOnuOTransceiversEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "ONU optical transceivers entry."
        INDEX   { ifIndex, zxAnPonOnuIndex }
        ::= { zxAnPonRmOnuOTransceiversTable 1 }

    ZxAnPonRmOnuOTransceiversEntry ::=
        SEQUENCE {            
            zxAnPonRmOnuOTSpeedMode              INTEGER,
            zxAnPonRmOnuOTOdnType                INTEGER,
            zxAnPonRmOnuOTApplicableDevType      INTEGER,            
            zxAnPonRmOnuOTEncapsulationType      INTEGER,
            zxAnPonRmOnuOTTxWavelength           Integer32,
            zxAnPonRmOnuOTRxWavelength           Integer32,
            zxAnPonRmOnuOTVendorName             DisplayString,
            zxAnPonRmOnuOTVendorPn               DisplayString,
            zxAnPonRmOnuOTVendorSn               DisplayString
            } 
    
    zxAnPonRmOnuOTSpeedMode OBJECT-TYPE
        SYNTAX      INTEGER{
            gpon(1),
            xgpon(2),
            xgspon(3),
            epon(11),
            oneGAndTenGEpon(12),
            tenGAnd10GEpon(13),
            unknown(255)
        }
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "ONU optical transceivers speed mode.
             The following values are valid for GPON:
                 gpon(1): GPON,
                 xgpon(2): XG-PON,
                 xgspon(3): XGS-PON,
                 unknown(255): unknown.
             The following values are valid for EPON:
                 oneG(11): EPON,
                 oneGAndTenGEpon(12): 1G/10G-EPON, 
                 tenGAnd10GEpon(13): 10G/10G-EPON,
                 unknown(255): unknown."
        ::= { zxAnPonRmOnuOTransceiversEntry 1 }  
        
    zxAnPonRmOnuOTOdnType OBJECT-TYPE
        SYNTAX      INTEGER{
            classBplusAndCplus(1),
            n1AndN2aAndE1AndE2a(2),
            n2bAndE2b(3),
            n1AndN2AndE1(4),
            px20(51),
            px20plus(52),
            prx30(53),
            pr30(54),
            prx40(55),
            pr40(56),
            unknown(255)            
        }
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "ONU optical transceivers ODN type.
             The following values are valid for GPON:
                 classBplusAndCplus(1): Class B+/C+,
                 n1AndN2aAndE1AndE2a(2): N1/N2a/E1/E2a,
                 n2bAndE2b(3): N2b/E2b,
                 n1AndN2AndE1(4): N1/N2/E1.
             The following values are valid for EPON:
                 px20(51): PX20,
                 px20plus(52): PX20+,
                 prx30(53): PRX30,
                 pr30(54): PR30,
                 prx40(55): PRX40,
                 pr40(56): PR40,
                 unknown(255): unknown."
        ::= { zxAnPonRmOnuOTransceiversEntry 2 }

    zxAnPonRmOnuOTApplicableDevType OBJECT-TYPE
        SYNTAX      INTEGER{
            onu(1),
            unknown(255)
        }
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "ONU optical transceivers applicable device type."
        ::= { zxAnPonRmOnuOTransceiversEntry 3 } 

    zxAnPonRmOnuOTEncapsulationType OBJECT-TYPE
        SYNTAX      INTEGER{
            sfpAndsfpplus(1),
            xfp(2),
            unknown(255)
        }
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "ONU optical transceivers encapsulation type.
             sfpAndsfpplus(1): SFP/SFP+,
             xfp(2): XFP,
             unknown(255): unknown."
        ::= { zxAnPonRmOnuOTransceiversEntry 4 } 

    zxAnPonRmOnuOTTxWavelength OBJECT-TYPE
        SYNTAX      Integer32(0..65535)
        UNITS       "nm"
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "ONU optical transceivers transmitted wavelength."
        ::= { zxAnPonRmOnuOTransceiversEntry 5 }
        
    zxAnPonRmOnuOTRxWavelength OBJECT-TYPE
        SYNTAX      Integer32(0..65535)
        UNITS       "nm"
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "ONU optical transceivers received wavelength."
        ::= { zxAnPonRmOnuOTransceiversEntry 6 } 

    zxAnPonRmOnuOTVendorName OBJECT-TYPE
        SYNTAX      DisplayString (SIZE(0..16))
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "ONU optical transceivers vendor name."
        ::= { zxAnPonRmOnuOTransceiversEntry 7 }
         
    zxAnPonRmOnuOTVendorPn OBJECT-TYPE
        SYNTAX      DisplayString (SIZE(0..16))
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "ONU optical transceivers vendor part number."
        ::= { zxAnPonRmOnuOTransceiversEntry 8 }
                
    zxAnPonRmOnuOTVendorSn OBJECT-TYPE
        SYNTAX      DisplayString (SIZE(0..16))
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "ONU optical transceivers vendor serial number."
        ::= { zxAnPonRmOnuOTransceiversEntry 9 }
        

    
                                                                                            
  
