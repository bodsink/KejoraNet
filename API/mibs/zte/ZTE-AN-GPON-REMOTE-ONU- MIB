-- PON ONU POTS calling data record table                                                 
-- zxAnPonRmPotsCallDataRecordTable                                                      
--------------------------------------------------------------------------------  
    zxAnPonRmPotsCallDataRecordTable    OBJECT-TYPE
        SYNTAX      SEQUENCE OF ZxAnPonRmPotsCallDataRecordEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "PON ONU POTS calling data record table."
        ::= { zxAnPonRmOnuVoipObjects 2 }

    zxAnPonRmPotsCallDataRecordEntry OBJECT-TYPE
        SYNTAX      ZxAnPonRmPotsCallDataRecordEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "An entry in zxAnPonRmPotsCallDataRecordTable."
        INDEX   { ifIndex, zxAnPonOnuIndex,
         zxAnPonRmOnuUniIndex, zxAnPonRmPotsCdrIndex }
        ::= { zxAnPonRmPotsCallDataRecordTable 1 }
        
    ZxAnPonRmPotsCallDataRecordEntry ::=
        SEQUENCE {
            zxAnPonRmOnuUniIndex            ZxAnPonRmOnuUniIndex,
            zxAnPonRmPotsCdrIndex           Integer32,
            zxAnPonRmPotsCdrTotalNum        Integer32,
            zxAnPonRmPotsCdrSequenceNum     Integer32,
            zxAnPonRmPotsCdrTime            DateAndTime,
            zxAnPonRmPotsCdrDuration        Integer32,
            zxAnPonRmPotsCdrCalleeNumber    DisplayString,
            zxAnPonRmPotsCdrCallerNumber    DisplayString,
            zxAnPonRmPotsCdrTxRtpPkts       Integer32,
            zxAnPonRmPotsCdrRxRtpPkts       Integer32,
            zxAnPonRmPotsCdrLostRtpPkts     Integer32,
            zxAnPonRmPotsCdrDiscardRtpPkts  Integer32,
            zxAnPonRmPotsCdrBufOverCount    Integer32,
            zxAnPonRmPotsCdrBufEmptyCount   Integer32,
            zxAnPonRmPotsCdrRxAvgJitter     Integer32,
            zxAnPonRmPotsCdrFarEndRtcp      INTEGER,
            zxAnPonRmPotsCdrRxPeakJitter    Integer32,
            zxAnPonRmPotsCdrRxAvgJtrBufDep  Integer32,
            zxAnPonRmPotsCdrFarEndRtcpXr    INTEGER,
            zxAnPonRmPotsCdrPeakRtDelay     Integer32,
            zxAnPonRmPotsCdrAvgRtDelay      Integer32,
            zxAnPonRmPotsCdrAvgMos          Integer32
        }
                
    zxAnPonRmOnuUniIndex OBJECT-TYPE
        SYNTAX      ZxAnPonRmOnuUniIndex
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This attribute indicates the index of UNI in ONU."
        ::= { zxAnPonRmPotsCallDataRecordEntry 1}    
            
    zxAnPonRmPotsCdrIndex OBJECT-TYPE
        SYNTAX      Integer32
        MAX-ACCESS  not-accessible
