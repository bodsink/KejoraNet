ZTE-AN-GPON-ONUPRF-MIB DEFINITIONS ::= BEGIN 
    IMPORTS
    
        MODULE-IDENTITY, OBJECT-TYPE, Integer32  FROM SNMPv2-SMI
        Unsigned32                               FROM SNMPv2-SMI
        RowStatus, DisplayString                 FROM SNMPv2-TC
        NOTIFICATION-GROUP, MODULE-COMPLIANCE,
        OBJECT-GROUP                             FROM SNMPv2-CONF        
        zxAnPon                                  FROM ZTE-AN-SMI        
        ZxAnGponOnuTcontIndex                    FROM ZTE-AN-PON-TC-MIB
        ZxAnGponOnuGemPortIndex                  FROM ZTE-AN-PON-TC-MIB
        ZxAnGponOnuVportIndex                    FROM ZTE-AN-PON-TC-MIB
        zxAnPonRmOnuIfIndex                      FROM ZTE-AN-PON-REMOTE-ONU-MIB
        InetAddress,InetAddressType              FROM INET-ADDRESS-MIB
        InetAddressPrefixLength                  FROM INET-ADDRESS-MIB;

    zxAnGponOnuPrfMib MODULE-IDENTITY 
        LAST-UPDATED    "201706170000Z"
        ORGANIZATION    "ZTE Corporation"
        CONTACT-INFO    "Mail: shan.jun@zte.com.cn
                         Tel : +86-21-68897250"
        DESCRIPTION     "The MIB module for the management of GPON ONU profile."
        
        --  Revision history
        REVISION        "201706170000Z"
        DESCRIPTION
            "Initial version of this MIB module."
        ::= { zxAnPon 21 }

