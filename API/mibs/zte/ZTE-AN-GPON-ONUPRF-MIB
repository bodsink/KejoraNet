--------------------------------------------------------------------------------
-- zxAnGponOPTcontTable
--------------------------------------------------------------------------------
    zxAnGponOPTcontTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF ZxAnGponOPTcontEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The ONU profile T-CONT table."
        ::= { zxAnGponOnuLinePrfObjects 2 }

    zxAnGponOPTcontEntry OBJECT-TYPE
        SYNTAX      ZxAnGponOPTcontEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The ONU profile T-CONT entry."
        INDEX   { zxAnGponOPName, zxAnGponOPTcontIndex }
        ::= { zxAnGponOPTcontTable 1 }

    ZxAnGponOPTcontEntry ::=
        SEQUENCE {
            zxAnGponOPName                   DisplayString,
            zxAnGponOPTcontIndex             ZxAnGponOnuTcontIndex,
            zxAnGponOPTcontName              DisplayString,
            zxAnGponOPTcontBwPrfName         DisplayString,
            zxAnGponOPTcontDbaGapMode        INTEGER,
            zxAnGponOPTcontRowStatus         RowStatus
        }

    zxAnGponOPName       OBJECT-TYPE
        SYNTAX      DisplayString(SIZE (1..64))
        MAX-ACCESS  not-accessible
        STATUS      current 
        DESCRIPTION 
            "Name of ONU profile."
        ::= { zxAnGponOPTcontEntry 1 }
        
    zxAnGponOPTcontIndex       OBJECT-TYPE
        SYNTAX      ZxAnGponOnuTcontIndex
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "An arbitrary (unique) integer for identifying an entry
             in the table, from 1 to upper limit supported T-CONT count of
             ONU type."
        ::= { zxAnGponOPTcontEntry 2}

    zxAnGponOPTcontName       OBJECT-TYPE
        SYNTAX      DisplayString(SIZE (0..64))
        MAX-ACCESS  read-create
        STATUS      current 
        DESCRIPTION 
            "Name of the T-CONT."
        ::= { zxAnGponOPTcontEntry 3 }
        
    zxAnGponOPTcontBwPrfName       OBJECT-TYPE
        SYNTAX      DisplayString(SIZE(0..64))
        MAX-ACCESS  read-create
        STATUS      current 
        DESCRIPTION 
            "The name of profile used by T-CONT bandwidth configuration."
        ::= { zxAnGponOPTcontEntry 4 }
              
    zxAnGponOPTcontDbaGapMode        OBJECT-TYPE
        SYNTAX     INTEGER {
            mode0(1),
            mode1(2),
            mode2(3),
            mode3(4),
            default(11)
        }
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "The DBA gap mode. 
             For GPON ONU, the explanations of these modes are as follows:
               mode0 is one frame, 
               mode1 is two frames, 
               mode2 is four frames, 
               mode3 is eight frames.
               default is the system default policy.
             For example, 'two frames' indicates that T-CONT will perform DBA 
             in a gap of two data frames. "
        ::= { zxAnGponOPTcontEntry 5 }
        
    zxAnGponOPTcontRowStatus       OBJECT-TYPE
        SYNTAX      RowStatus
        MAX-ACCESS  read-create
        STATUS      current 
        DESCRIPTION 
            "The status of this conceptual row.
        
             To create a row in this table, a manager must
             set this object to createAndGo(4).

             To delete a row in this table, a manager must
             set this object to destroy(6).
             
             To query this object, always return 1. "
        ::= { zxAnGponOPTcontEntry 50 }
       
--------------------------------------------------------------------------------
-- zxAnGponOPGemPortTable
--------------------------------------------------------------------------------
    zxAnGponOPGemPortTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF ZxAnGponOPGemPortEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The ONU profile GEM port table."
        ::= { zxAnGponOnuLinePrfObjects 3 }

    zxAnGponOPGemPortEntry OBJECT-TYPE
        SYNTAX      ZxAnGponOPGemPortEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The ONU profile GEM port entry."
        INDEX   { zxAnGponOPName, zxAnGponOPGemPortIndex }
        ::= { zxAnGponOPGemPortTable 1 }

    ZxAnGponOPGemPortEntry ::=
        SEQUENCE {
            zxAnGponOPGemPortIndex                   ZxAnGponOnuGemPortIndex,
            zxAnGponOPGemPortName                    DisplayString,
            zxAnGponOPGemPortTcontIndex              ZxAnGponOnuTcontIndex,
            zxAnGponOPGemPortQueueOfTcont            Integer32,
            zxAnGponOPGemPortEncrypt                 INTEGER,
            zxAnGponOPGemPortRowStatus               RowStatus
        }

    zxAnGponOPGemPortIndex OBJECT-TYPE
        SYNTAX      ZxAnGponOnuGemPortIndex
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "An arbitrary (unique) integer for identifying an entry
             in the table, from 1 to upper limit supported GEM port count of
             ONU type."
        ::= { zxAnGponOPGemPortEntry 1}

    zxAnGponOPGemPortName       OBJECT-TYPE
        SYNTAX      DisplayString(SIZE (0..64))
        MAX-ACCESS  read-create
        STATUS      current 
        DESCRIPTION 
            "Name of the GEM port."
        ::= { zxAnGponOPGemPortEntry 2 }

    zxAnGponOPGemPortTcontIndex OBJECT-TYPE
        SYNTAX      ZxAnGponOnuTcontIndex
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "It is equal to the 'zxAnGponOPTcontIndex' 
             of 'zxAnGponOPTcontTable'."
        ::= { zxAnGponOPGemPortEntry 3}

    zxAnGponOPGemPortQueueOfTcont OBJECT-TYPE
        SYNTAX      Integer32(0..7)
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "The queue (index from 0 to 7) of T-CONT for this GEM port.
             Queue 7 is the highest priority queue."
        DEFVAL { 0 }
        ::= { zxAnGponOPGemPortEntry 4}
        
     zxAnGponOPGemPortEncrypt OBJECT-TYPE
        SYNTAX      INTEGER {
            downstreamEncrypt(1),
            noEncrypt(2),
            bothDirectEncrypt(3)
        }
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "The encrypt state of this GEM port."
        DEFVAL { noEncrypt }
        ::= { zxAnGponOPGemPortEntry 5}
                
    zxAnGponOPGemPortRowStatus       OBJECT-TYPE
        SYNTAX      RowStatus
        MAX-ACCESS  read-create
        STATUS      current 
        DESCRIPTION 
            "The status of this conceptual row.
        
             To create a row in this table, a manager must
             set this object to createAndGo(4).
            
             In particular, a newly created row cannot be created
             until the corresponding 'zxAnGponOPGemPortTcontIndex'
             has been set.
             
             To delete a row in this table, a manager must
             set this object to destroy(6).
             
             To query this object, always return 1. "
        ::= { zxAnGponOPGemPortEntry 50 }        

-------------------------------------------------------------------------------
-- zxAnGponOPMappingTable
-------------------------------------------------------------------------------
    zxAnGponOPMappingTable    OBJECT-TYPE
        SYNTAX      SEQUENCE OF ZxAnGponOPMappingEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The ONU profile mapping table."
        REFERENCE
            "G.988"
        ::= { zxAnGponOnuLinePrfObjects 4 }

    zxAnGponOPMappingEntry OBJECT-TYPE
        SYNTAX      ZxAnGponOPMappingEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The ONU profile mapping entry."
        INDEX   { zxAnGponOPName, zxAnGponOPMappingName }
        ::= { zxAnGponOPMappingTable 1 }

    ZxAnGponOPMappingEntry ::=
        SEQUENCE {
            zxAnGponOPMappingName                       DisplayString,
            zxAnGponOPMappingGemPort                    Integer32,
            zxAnGponOPMappingIfId                       Integer32,
            zxAnGponOPMappingType                       INTEGER,
            zxAnGponOPMappingCos                        Integer32,
            zxAnGponOPMappingVid                        OCTET STRING,
            zxAnGponOPMappingRowStatus                  RowStatus
        }

    zxAnGponOPMappingName OBJECT-TYPE
        SYNTAX      DisplayString(SIZE(1..32))
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "Name of service."
        REFERENCE
            "G.988"
        ::= { zxAnGponOPMappingEntry 1 }

    zxAnGponOPMappingGemPort OBJECT-TYPE
        SYNTAX      Integer32(1..255)
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "GEM port index of service."
        REFERENCE
            "G.988"
        ::= { zxAnGponOPMappingEntry 2 }

    zxAnGponOPMappingIfId OBJECT-TYPE
        SYNTAX      Integer32
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "4bytes value contains:type (MSB 2bytes)/ifId (LSB 2bytes).
             type   means
             0      bridge
             1      ethuni
             2      iphost
             3      wifiuni
             4      veip
             5      wan

             If type is 'bridge', then the 2-byte ifId has no meaning, just keep
             it zero;
             If type is 'ethuni', then the 2-byte ifId contains:
             slot (MSB 1byte)/port (LSB 1byte);
             If type is 'iphost', then the 2-byte ifId means the ID of iphost,
             range from 1 to 255;
             If type is 'wifiuni', then the 2-byte ifId contains:
             slot (MSB 1byte)/port (LSB 1byte);
             If type is 'veip', then the 2-byte ifId means the ID of VEIP,
             range from 1 to 255;
             If type is 'wan', then the 2-byte ifId means the ID of WAN,
             range from 1 to 255.
             "
        REFERENCE
            "G.988"
        ::= { zxAnGponOPMappingEntry 3 }

    zxAnGponOPMappingType OBJECT-TYPE
        SYNTAX      INTEGER{
            cos(1),
            vlan(2),
            cosAndVlan(3),
            transparent(4),
            untag(5)
        }
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "Map type of service.
             - cos: mapping to GEM port by CoS value
             - vlan: mapping to GEM port by VLAN value
             - cosAndVlan: mapping to GEM port by both cos and VLAN value
             - transparent: mapping to GEM port transparently
             - untag: mapping to GEMP port for untag frames"
        REFERENCE
            "G.988"
        ::= { zxAnGponOPMappingEntry 4 }

    zxAnGponOPMappingCos OBJECT-TYPE
        SYNTAX      Integer32(0..255)
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "A bitfield defines the frames with the priority related with this
             service. MSB stands for priority 7, LSB stands for priority 0.
             A bit value of 0 specifies that frames with the associated priority
             are to be discarded."
        REFERENCE
            "G.988"
        DEFVAL { 0 }
        ::= { zxAnGponOPMappingEntry 5 }

    zxAnGponOPMappingVid OBJECT-TYPE
        SYNTAX      OCTET STRING(SIZE(24))
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "Each 2 bytes stand for the VLAN ID related with this service.
             The first N*2 bytes are valid, until VLAN ID=0."
        REFERENCE
            "G.988"
        ::= { zxAnGponOPMappingEntry 6 }

    zxAnGponOPMappingRowStatus OBJECT-TYPE
        SYNTAX      RowStatus
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "The status of this conceptual row.
             The state of this conceptual row only support
             'not exist' and  'active'.
             The action of this conceptual row only support
             'createAndGo' and 'destroy'.

             To create a row in this table, a manager must
             set this object to createAndGo(4), and must comply
             with the multi-variables usage rules in 'zxAnGponOPMappingEntry'
             description.

             To delete a row in this table, a manager must
             set this object to destroy(6).

             To query this object, always return 1. "
        REFERENCE
            "G.988"
        ::= { zxAnGponOPMappingEntry 50 }
        
-------------------------------------------------------------------------------
-- zxAnGponOPOnuConfTable
-------------------------------------------------------------------------------
    zxAnGponOPOnuConfTable    OBJECT-TYPE
        SYNTAX      SEQUENCE OF ZxAnGponOPOnuConfEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The ONU profile ONU configuration table."
        ::= { zxAnGponOnuLinePrfObjects 5 }

    zxAnGponOPOnuConfEntry OBJECT-TYPE
        SYNTAX      ZxAnGponOPOnuConfEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The ONU profile ONU configuration entry."
        INDEX   { zxAnGponOPName }
        ::= { zxAnGponOPOnuConfTable 1 }

    ZxAnGponOPOnuConfEntry ::=
        SEQUENCE {
            zxAnGponOPOnuConfVportMode           INTEGER,  
            zxAnGponOPOnuConfUsFecEnable         INTEGER
        }
    
    zxAnGponOPOnuConfVportMode OBJECT-TYPE
        SYNTAX      INTEGER{
            gemport(1),
            manual(3)
        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
             "The VPort mode.
              gemport(1): a related VPort will be created automatically when a 
                          GEM port is created.
              manual(3): VPort will be created manually."
        ::= { zxAnGponOPOnuConfEntry 1 } 

    zxAnGponOPOnuConfUsFecEnable OBJECT-TYPE
        SYNTAX      INTEGER{
            enabled(1),
            disabled(2),
            auto(3),
            default(4)                    
        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "Enables or disables the ONU upstream FEC function.
             enabled(1): enable the ONU upstream FEC function.
             disabled(2): disable the ONU upstream FEC function.
             auto(3): system will enable or disable the ONU upstream FEC 
                      function automatically according to the system automation 
                      rule.
             default(4): system will enable or disable the ONU upstream FEC 
                      function according to the system default rule."        
        DEFVAL { default }
        ::= { zxAnGponOPOnuConfEntry 2 }
        
--------------------------------------------------------------------------------
-- zxAnGponOPBpConfTable
--------------------------------------------------------------------------------
    zxAnGponOPBpConfTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF ZxAnGponOPBpConfEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The ONU profile VPort table." 
        ::= { zxAnGponOnuLinePrfObjects 6 }

    zxAnGponOPBpConfEntry OBJECT-TYPE
        SYNTAX      ZxAnGponOPBpConfEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The ONU profile VPort entry."
        INDEX   { zxAnGponOPName, zxAnGponOPBpVportId }
        ::= { zxAnGponOPBpConfTable 1 }

    ZxAnGponOPBpConfEntry ::=
        SEQUENCE {
            zxAnGponOPBpVportId               ZxAnGponOnuVportIndex,
            zxAnGponOPBpName                  DisplayString,
            zxAnGponOPBpMapType               INTEGER,
            zxAnGponOPBpRowStatus             RowStatus
        }
        
    zxAnGponOPBpVportId OBJECT-TYPE
        SYNTAX      ZxAnGponOnuVportIndex
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "VPort ID."
        ::= { zxAnGponOPBpConfEntry 1}  
          
    zxAnGponOPBpName OBJECT-TYPE
        SYNTAX      DisplayString(SIZE(0..64))
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "Name of the VPort."
        ::= { zxAnGponOPBpConfEntry 2}
        
    zxAnGponOPBpMapType OBJECT-TYPE
        SYNTAX      INTEGER{
            cos(1),
            vlan(2),
            cosPlusVlan(3),
            oneToOne(4)
        }
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "Map type of GEM port and VPort.
             cos(1): mapping by CoS value.
             vlan(2): mapping by VLAN value.
             cosPlusVlan(3): mapping by CoS and VLAN value.
             oneToOne(4): mapping is 1:1 between GEM port and VPort."            
        ::= { zxAnGponOPBpConfEntry 3}
   
    zxAnGponOPBpRowStatus OBJECT-TYPE
        SYNTAX      RowStatus
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "The status of this conceptual row.

             The supported actions of this conceptual row only include:

             To create a row in this table, a manager must
             set this object to createAndGo(4).

             To delete a row in this table, a manager must
             set this object to destroy(6).
            
             To query this object, always return 1. "
        ::= { zxAnGponOPBpConfEntry 50}

--------------------------------------------------------------------------------
-- zxAnGponOPBpMapConfTable
--------------------------------------------------------------------------------
    zxAnGponOPBpMapConfTable OBJECT-TYPE
        SYNTAX      SEQUENCE OF ZxAnGponOPBpMapConfEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The ONU profile VPort mapping table." 
        ::= { zxAnGponOnuLinePrfObjects 7 }

    zxAnGponOPBpMapConfEntry OBJECT-TYPE
        SYNTAX      ZxAnGponOPBpMapConfEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The ONU profile VPort mapping entry."
        INDEX   { zxAnGponOPName, zxAnGponOPBpVportId, zxAnGponOPGemPortIndex,
                  zxAnGponOPBpMapConfCos, zxAnGponOPBpMapConfVid}
        ::= { zxAnGponOPBpMapConfTable 1 }

    ZxAnGponOPBpMapConfEntry ::=
        SEQUENCE {
            zxAnGponOPBpMapConfCos         Integer32,
            zxAnGponOPBpMapConfVid         Integer32,
            zxAnGponOPBpMapConfRowStatus   RowStatus
        }

    zxAnGponOPBpMapConfCos OBJECT-TYPE
        SYNTAX      Integer32(0..7)
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "A CoS value (0..7) referenced by a GEM port mapped to a VPort."
        ::= { zxAnGponOPBpMapConfEntry 1}
        
    zxAnGponOPBpMapConfVid OBJECT-TYPE
        SYNTAX      Integer32(0..4094|65535)
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "A VLAN ID referenced by a GEM port mapped to a VPort. 
             The value 65535 indicates the untagged frames.
             When VPort mapping type is oneToOne(4) or cos(1), this value is 0."
        ::= { zxAnGponOPBpMapConfEntry 2}
        
    zxAnGponOPBpMapConfRowStatus OBJECT-TYPE
        SYNTAX      RowStatus
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "The status of this conceptual row.
            
             The supported actions of this conceptual row only include:

             To create a row in this table, a manager must
             set this object to createAndGo(4).

             To delete a row in this table, a manager must
             set this object to destroy(6).
             
             To query this object, always return 1. "
        ::= { zxAnGponOPBpMapConfEntry 50}

