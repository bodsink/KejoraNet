        REFERENCE
            "G.988 section 9.4.1 IP host config data 
             and 9.4.5 IPv6 host config data."
        ::= { zxAnGponOnuServicePrfObjects 11 }

    zxAnGponOPWanIpConfEntry OBJECT-TYPE
        SYNTAX      ZxAnGponOPWanIpConfEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The ONU profile WAN IP configuration entry."             
        INDEX   { zxAnGponOPName, zxAnGponOPWanIpIndex }
        ::= { zxAnGponOPWanIpConfTable 1 }

    ZxAnGponOPWanIpConfEntry ::=
        SEQUENCE {
            zxAnGponOPWanIpIndex            Integer32,
            zxAnGponOPWanIpMode             INTEGER,
            zxAnGponOPWanVlanPrf            DisplayString,
            zxAnGponOPWanHostId             Integer32,
            zxAnGponOPWanOnuIdentifier      DisplayString,
            zxAnGponOPWanPppoeAuthMode      INTEGER,
            zxAnGponOPWanPppoeUsername      DisplayString,
            zxAnGponOPWanPppoePassword      DisplayString,
            zxAnGponOPWanRspPing            INTEGER,
            zxAnGponOPWanRspTraceRoute      INTEGER,
            zxAnGponOPWanRsEnable           INTEGER,            
            zxAnGponOPWanOnLinkPrefix       OCTET STRING, 
            zxAnGponOPWanPppoeServiceName   DisplayString,
            zxAnGponOPWanIpConfRowStatus    RowStatus
        }

    zxAnGponOPWanIpIndex OBJECT-TYPE
        SYNTAX      Integer32(1..255)
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The ID of WAN IP."
        ::= { zxAnGponOPWanIpConfEntry 1}

    zxAnGponOPWanIpMode OBJECT-TYPE
        SYNTAX      INTEGER{
             dhcp(2),
             pppoe(3)
        }
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "IP address configuration mode."
        DEFVAL { dhcp }
        ::= { zxAnGponOPWanIpConfEntry 2 }

    zxAnGponOPWanVlanPrf OBJECT-TYPE
        SYNTAX      DisplayString(SIZE(1..64))
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "The name of table 'zxAnGponOPVlanPrfTable'."
        ::= { zxAnGponOPWanIpConfEntry 3 }

    zxAnGponOPWanHostId  OBJECT-TYPE
        SYNTAX      Integer32(0..33023)
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "The ONU IP host ID. The value 0 indicates this attribute
             is not configured.
             The following IP host ID format is defined:             
             MSB : Most Significant Byte
             LSB : Least Significant Byte        
             |      MSB     |    Byte 2    |       Byte 1      |    LSB     |
             |--------------|-----------------------------------------------|
             |              |              |IP-type | reserved | host ID    |
             |     (8bits)  |  (8bits)     | (1bit) | (7bits)  | (8bits)    |
             |--------------------------------------------------------------|             
             IP-type: the IP type of host ID(1 bit)
             0 represents IPv4
             1 represents IPv6"              
        REFERENCE
            "G.988 section 9.4.1 IP host config data 
             and 9.4.5 IPv6 host config data."
        DEFVAL { 0 }     
        ::= { zxAnGponOPWanIpConfEntry 4 }

    zxAnGponOPWanOnuIdentifier OBJECT-TYPE
        SYNTAX      DisplayString(SIZE(0..25))
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "A unique ONU identifier string. This string, if set is provided as
             part of the DHCP request to allow an alternative to MAC Address in
             retrieving the DHCP parameters of the specified ONU.
             This attribute is related to 'zxAnGponOPWanIpMode'.
             and it is valid only when 'zxAnGponOPWanIpMode' is DHCP.
             Its default value is null bytes."
        REFERENCE
            "G.988 section 9.4.1 IP host config data"
        ::= { zxAnGponOPWanIpConfEntry 5 }

    zxAnGponOPWanPppoeAuthMode OBJECT-TYPE
        SYNTAX      INTEGER{
            auto(1),
            chap(2),
            pap(3)
        }
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "This attribute is related to 'zxAnGponOPWanIpMode', and
             it is valid only when 'zxAnGponOPWanIpMode' is PPPoE."
        DEFVAL { auto }
        ::= { zxAnGponOPWanIpConfEntry 6 }

    zxAnGponOPWanPppoeUsername OBJECT-TYPE
        SYNTAX      DisplayString(SIZE(0..128))
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "This attribute is related to 'zxAnGponOPWanIpMode', and it is
             valid only when 'zxAnGponOPWanIpMode' is PPPoE."
        ::= { zxAnGponOPWanIpConfEntry 7 }

    zxAnGponOPWanPppoePassword OBJECT-TYPE
        SYNTAX      DisplayString(SIZE(0..25))
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "This attribute is related to 'zxAnGponOPWanIpMode', and it is
             valid only when 'zxAnGponOPWanIpMode' is PPPoE."
        ::= { zxAnGponOPWanIpConfEntry 8 }

    zxAnGponOPWanRspPing OBJECT-TYPE
        SYNTAX      INTEGER{
            enabled(1),
            disabled(2)
        }
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "This attribute indicates whether respond to pings enabled."
        DEFVAL { enabled }
        ::= { zxAnGponOPWanIpConfEntry 9 }

    zxAnGponOPWanRspTraceRoute OBJECT-TYPE
        SYNTAX      INTEGER{
            enabled(1),
            disabled(2)
        }
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "This attribute indicates whether respond to trace route enabled.
             This attribute is only valid for IPv4 and this value is
             disabled(2) for IPv6."
        DEFVAL { disabled }
        ::= { zxAnGponOPWanIpConfEntry 10 }

    zxAnGponOPWanRsEnable OBJECT-TYPE
        SYNTAX      INTEGER{
            enabled(1),
            disabled(2)
        }
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION            
            "This attribute indicates whether respond to router 
             solicitation (RS) messages enabled. 
             This attribute is only valid for IPv6 and this value is
             disabled(2) for IPv4."
        REFERENCE
             "G.988 section 9.4.5 IPv6 host config data."
        DEFVAL { disabled }
        ::= { zxAnGponOPWanIpConfEntry 11 }
        
    zxAnGponOPWanOnLinkPrefix OBJECT-TYPE
        SYNTAX      OCTET STRING (SIZE (17))
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "It is the manually provisioned on-link prefix used for destination 
             IPv6 addresses of IPv6 host services. The attribute is structured 
             as follows: 
             Prefix length, number of leading bits in the prefix that are valid 
             (1 byte). 
             Prefix (16 bytes).
             This attribute is only valid for IPv6 and this value is 0x0 
             for IPv4."
        REFERENCE
            "G.988 section 9.4.5 IPv6 host config data"
        DEFVAL { '00'H  }            
        ::= { zxAnGponOPWanIpConfEntry 12 }    

    zxAnGponOPWanPppoeServiceName OBJECT-TYPE
        SYNTAX      DisplayString(SIZE(0..60))
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "The PPPoE service name."
        ::= { zxAnGponOPWanIpConfEntry 13 }
                
    zxAnGponOPWanIpConfRowStatus OBJECT-TYPE
        SYNTAX      RowStatus
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "The status of this conceptual row.

             To create a row in this table, a manager must
             set this object to createAndGo(4).

             To delete a row in this table, a manager must
             set this object to destroy(6).

             To query this object, always return 1. "
        ::= { zxAnGponOPWanIpConfEntry 50 }
                
