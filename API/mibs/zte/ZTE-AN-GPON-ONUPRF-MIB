-- zxAnGponOPTcontQueueSchedTable
-------------------------------------------------------------------------------
    zxAnGponOPTcontQueueSchedTable    OBJECT-TYPE
        SYNTAX      SEQUENCE OF ZxAnGponOPTcontQueueSchedEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The ONU profile T-CONT queue schedule table."            
        ::= { zxAnGponOnuLinePrfObjects 8 }

    zxAnGponOPTcontQueueSchedEntry OBJECT-TYPE
        SYNTAX      ZxAnGponOPTcontQueueSchedEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The ONU profile T-CONT queue schedule entry."
        INDEX   { zxAnGponOPName, zxAnGponOPTcontIndex }
        ::= { zxAnGponOPTcontQueueSchedTable 1 }

    ZxAnGponOPTcontQueueSchedEntry ::=
        SEQUENCE {
            zxAnGponOPTcontQueueSchedPrfName       DisplayString
        }
          
    zxAnGponOPTcontQueueSchedPrfName OBJECT-TYPE
        SYNTAX      DisplayString(SIZE(0..64))
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The T-CONT queue schedule profile name, this profile is 
             defined in zxAnGponRmTrafficSchedPrfTable. The empty string
             indicates that no profile is bound to this T-CONT."
        ::= { zxAnGponOPTcontQueueSchedEntry 1 }

