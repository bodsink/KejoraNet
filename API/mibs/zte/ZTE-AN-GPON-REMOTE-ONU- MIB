        DESCRIPTION
            "Buffer overflow count."
        ::= { zxAnPonRmPotsCallDataRecordEntry 13 }  
        
    zxAnPonRmPotsCdrBufEmptyCount OBJECT-TYPE
        SYNTAX      Integer32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Buffer empty count."
        ::= { zxAnPonRmPotsCallDataRecordEntry 14 }  
        
    zxAnPonRmPotsCdrRxAvgJitter OBJECT-TYPE
        SYNTAX      Integer32
        UNITS       "Milliseconds"
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The average jitter for received packets."
        ::= { zxAnPonRmPotsCallDataRecordEntry 15 }  
        
    zxAnPonRmPotsCdrFarEndRtcp OBJECT-TYPE
        SYNTAX      INTEGER{
                    participated(1),
                    notParticipated(2)
                    } 
        MAX-ACCESS  read-only
        STATUS      current
