-- zxAnGponOPVoipConfTable
------------------------------------------------------------------------------
    zxAnGponOPVoipConfTable    OBJECT-TYPE
        SYNTAX      SEQUENCE OF ZxAnGponOPVoipConfEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The ONU profile VoIP configuration table."
        REFERENCE
            "G.988 section 9.9.18 VoIP config data"
        ::= { zxAnGponOnuServicePrfObjects  12 }

    zxAnGponOPVoipConfEntry OBJECT-TYPE
        SYNTAX      ZxAnGponOPVoipConfEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The ONU profile VoIP configuration entry."
        INDEX   { zxAnGponOPName }
        ::= { zxAnGponOPVoipConfTable 1 }

    ZxAnGponOPVoipConfEntry ::=
        SEQUENCE {
            zxAnGponOPVoipSignalProtoUsed                    INTEGER,
            zxAnGponOPVoipConfMethodUsed                     INTEGER,
            zxAnGponOPVoipDomainName                         DisplayString,
            zxAnGponOPVoipH248EquipName                      DisplayString,
            zxAnGponOPVoipH248RegMode                        INTEGER,
            zxAnGponOPVoipMgcRegMode                         INTEGER,
            zxAnGponOPVoipLineName                           DisplayString,
            zxAnGponOPVoipIpAllocMode                        INTEGER,
            zxAnGponOPVoipSvrAddr                            DisplayString,
            zxAnGponOPVoipSvrValidation                      INTEGER,
            zxAnGponOPVoipSvrUsername                        DisplayString,
            zxAnGponOPVoipSvrPassword                        DisplayString,
            zxAnGponOPVoipSvrRealm                           DisplayString,
            zxAnGponOPVoipSvrRetrieveFile                    INTEGER
        }

    zxAnGponOPVoipSignalProtoUsed OBJECT-TYPE
        SYNTAX      INTEGER{
            none(1),
            sip(2),
            h248(3),
            mgcp(4),
            nonOmci(5)
        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "This attribute indicates the type of VoIP signaling protocol used
             for ONU. Only one type of protocol is allowed. Valid values are:
             0x01 = None
             0x02 = SIP
             0x03 = H.248
             0x04 = MGCP
             0x05 = Selected by Non-OMCI."
        REFERENCE
            "G.988 section 9.9.18 VoIP config data"
        DEFVAL { none }
        ::= { zxAnGponOPVoipConfEntry 1 }

    zxAnGponOPVoipConfMethodUsed OBJECT-TYPE
        SYNTAX      INTEGER{
            default(1),
            omci(2),
            configFile(3),
            tr069(4),
            ietf(5)
        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "This attribute indicates the ONU configuration method.
             0x01 the ONU default - do not configure.
             0x02 indicate use of OMCI for VoIP service configuration of
                  the ONU.
             0x03 indicate use of configuration file retrieval for VoIP service
                  configuration of the ONU.
             0x04 indicate use of TR-69 for VoIP service configuration of
                  the ONU.
             0x05 indicate use of IETF sipping config framework for VoIP
                  service configuration of the ONU."
        REFERENCE
            "G.988 section 9.9.18 VoIP config data"
        DEFVAL { default }
        ::= { zxAnGponOPVoipConfEntry 2 }

    zxAnGponOPVoipDomainName  OBJECT-TYPE
        SYNTAX      DisplayString (SIZE (0..128))
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "This attribute indicates the ONU domain name that is used to locate 
             ONU by SS when the mode of MG registered to SS is using domain 
             name."
        REFERENCE
            "ZTE specific ME, MGC TID config data"
        ::= { zxAnGponOPVoipConfEntry 3 }

    zxAnGponOPVoipH248EquipName  OBJECT-TYPE
        SYNTAX      DisplayString (SIZE (0..25))
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "This attribute indicates the ONU equipment name that is used to 
             locate ONU by SS when the mode of MG registered to SS is using 
             equipment name."
        REFERENCE
            "ZTE specific ME, MGC TID config data"
        ::= { zxAnGponOPVoipConfEntry 4 }

    zxAnGponOPVoipH248RegMode OBJECT-TYPE
        SYNTAX      INTEGER{
            ipv4(1),
            domainName(2),
            equipmentName(3)
        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "This attribute indicates the MID flag."
        REFERENCE
            "ZTE specific ME, MGC TID config data"
        DEFVAL {ipv4}
        ::= { zxAnGponOPVoipConfEntry 5 }

    zxAnGponOPVoipMgcRegMode OBJECT-TYPE
        SYNTAX      INTEGER{
            wildcard(1),
            individual(2)
        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "This attribute indicates the register mode."
        REFERENCE
            "ZTE specific ME, MGC TID config data"
        DEFVAL {wildcard}
        ::= { zxAnGponOPVoipConfEntry 6 }

    zxAnGponOPVoipLineName  OBJECT-TYPE
        SYNTAX      DisplayString (SIZE (0..25))
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "This attribute indicates the line name sequence number.
             The default value is the empty string."
        REFERENCE
            "ZTE specific ME, MGC TID config data"
        ::= { zxAnGponOPVoipConfEntry 7 }

    zxAnGponOPVoipIpAllocMode OBJECT-TYPE
        SYNTAX      INTEGER{
            singleIp(1),
            dualIp(2),
            multiIp(3)
        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "This attribute indicates the VoIP allocate mode."
        DEFVAL { singleIp }
        ::= { zxAnGponOPVoipConfEntry 8 }

    zxAnGponOPVoipSvrAddr OBJECT-TYPE
        SYNTAX      DisplayString(SIZE (0..128))
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "This attribute indicates the address of the server to contact
             using the method indicated in the VoIP configuration method used
             attribute. This attribute is only relevant for non-OMCI
             configuration methods. If this attribute is set to a null pointer,
             no address is defined by this attribute. However, the address may
             be defined by other methods, such as deriving it from the ONU
             identifier attribute of the IP host configuration data ME and using 
             a well-known URI schema."
        REFERENCE
            "G.988 section 9.9.18 VoIP config data"
        ::= { zxAnGponOPVoipConfEntry 9 }

    zxAnGponOPVoipSvrValidation OBJECT-TYPE
        SYNTAX      INTEGER{
            disabled(1),
            md5(2),
            basic(3)
        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "This attribute indicates the user validation scheme."
        DEFVAL { disabled }
        ::= { zxAnGponOPVoipConfEntry 10 }

    zxAnGponOPVoipSvrUsername  OBJECT-TYPE
        SYNTAX      DisplayString (SIZE (0..50))
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "This attribute indicates the user name for validation."
        ::= { zxAnGponOPVoipConfEntry 11 }

    zxAnGponOPVoipSvrPassword  OBJECT-TYPE
        SYNTAX      DisplayString (SIZE (0..25))
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "This attribute indicates the password for validation."
        ::= { zxAnGponOPVoipConfEntry 12 }

    zxAnGponOPVoipSvrRealm  OBJECT-TYPE
        SYNTAX      DisplayString (SIZE (0..25))
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "This attribute indicates a string to be displayed to users so they 
             know which user name and password to use.  This string should 
             contain at least the name of the host performing the authentication 
             and might additionally indicate the collection of users who might 
             have access. An example might be 'registered_users@gotham. news
             .com'. The realm is a 'quoted-string' as specified in section 2.2 
             of the HTTP/1.1 specification."
        ::= { zxAnGponOPVoipConfEntry 13 }

    zxAnGponOPVoipSvrRetrieveFile  OBJECT-TYPE
        SYNTAX      INTEGER{
            enabled(1),
            disabled(2)
        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "This attribute indicates an action. It will be set independently
             to notify ONU to retrieve file again."
        ::= { zxAnGponOPVoipConfEntry 14 }

------------------------------------------------------------------------------
-- zxAnGponOPVoipIpConfTable
------------------------------------------------------------------------------
    zxAnGponOPVoipIpConfTable    OBJECT-TYPE
        SYNTAX      SEQUENCE OF ZxAnGponOPVoipIpConfEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The ONU profile VoIP IP configuration table."
        ::= { zxAnGponOnuServicePrfObjects 13 }

    zxAnGponOPVoipIpConfEntry OBJECT-TYPE
        SYNTAX      ZxAnGponOPVoipIpConfEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The ONU profile VoIP IP configuration entry."
        INDEX   { zxAnGponOPName, zxAnGponOPVoipIpSlotId, 
                  zxAnGponOPVoipIpMediaIndex }
        ::= { zxAnGponOPVoipIpConfTable 1 }

    ZxAnGponOPVoipIpConfEntry ::=
        SEQUENCE {
            zxAnGponOPVoipIpSlotId                    Integer32,
            zxAnGponOPVoipIpMediaIndex                INTEGER,
            zxAnGponOPVoipIpMode                      INTEGER,
            zxAnGponOPVoipIpPrf                       DisplayString,
            zxAnGponOPVoipIpVlanPrf                   DisplayString,
            zxAnGponOPVoipIpHostId                    Integer32,
            zxAnGponOPVoipIpOnuIdentifier             DisplayString,
            zxAnGponOPVoipIpAddrType                  InetAddressType,
            zxAnGponOPVoipIpAddr                      InetAddress,
            zxAnGponOPVoipIpAddrPfxLen                InetAddressPrefixLength,
            zxAnGponOPVoipIpPppoeMode                 INTEGER,
            zxAnGponOPVoipIpPppoeUsername             DisplayString,
            zxAnGponOPVoipIpPppoePassword             DisplayString,
            zxAnGponOPVoipIpRspPing                   INTEGER,
            zxAnGponOPVoipIpRspTraceRoute             INTEGER,
            zxAnGponOPVoipIpRsEnable                  INTEGER,            
            zxAnGponOPVoipIpOnLinkPrefix              OCTET STRING, 
            zxAnGponOPVoipIpPppoeServiceName          DisplayString,
            zxAnGponOPVoipIpConfRowStatus             RowStatus
        }

    zxAnGponOPVoipIpSlotId OBJECT-TYPE
        SYNTAX      Integer32(0..16)
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This attribute indicates the index of slot in ONU.
             If slot ID is zero, it means the ONU is box-type,
             which can not plug card."
        ::= { zxAnGponOPVoipIpConfEntry 1}

    zxAnGponOPVoipIpMediaIndex OBJECT-TYPE
        SYNTAX      INTEGER{
            signalAndMedia(1),
            media(2)
        }
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This attribute indicates the whether the separation of media
             and signaling. If 'media' is configured,The media will use a
             separation IP address, otherwise media and signaling will share
             the same IP address."
        ::= { zxAnGponOPVoipIpConfEntry 2}

    zxAnGponOPVoipIpMode OBJECT-TYPE
        SYNTAX      INTEGER{
            static(1),
            dhcp(2),
            pppoe(3)
        }
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "This attribute indicates the IP address configuration mode."
        DEFVAL { static }
        ::= { zxAnGponOPVoipIpConfEntry 3 }

    zxAnGponOPVoipIpPrf OBJECT-TYPE
        SYNTAX      DisplayString(SIZE(0..64))
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "This attribute indicates the VoIP IP profile Name, it refers to 
             'zxAnGponRmIpPrfName'. If no profile name is indicated, this 
             attribute is set to a empty string."
        ::= { zxAnGponOPVoipIpConfEntry 4 }

    zxAnGponOPVoipIpVlanPrf OBJECT-TYPE
        SYNTAX      DisplayString(SIZE(1..64))
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "This attribute indicates the VoIP IP VLAN profile Name, it refers 
             to 'zxAnGponRmVlanPrfName'."
        ::= { zxAnGponOPVoipIpConfEntry 5 }

    zxAnGponOPVoipIpHostId  OBJECT-TYPE
        SYNTAX      Integer32(0..33023)
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "This attribute indicates the ONU VoIP host ID. 
             The value 0 indicates this attribute is not configured.
             The following VoIP host ID format is defined:             
             MSB : Most Significant Byte
             LSB : Least Significant Byte        
             |      MSB     |    Byte 2    |       Byte 1      |    LSB     |
             |--------------|-----------------------------------------------|
             |              |              |IP-type | reserved | host ID    |
             |     (8bits)  |  (8bits)     | (1bit) | (7bits)  | (8bits)    |
             |--------------------------------------------------------------|             
             IP-type: the IP type of host ID(1 bit)
             0 represents IPv4
             1 represents IPv6" 
        REFERENCE
            "G.988 section 9.4.1 IP host config data 
             and 9.4.5 IPv6 host config data."
        DEFVAL { 0 }                
        ::= { zxAnGponOPVoipIpConfEntry 6 }

    zxAnGponOPVoipIpOnuIdentifier OBJECT-TYPE
        SYNTAX      DisplayString(SIZE(0..25))
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "This attribute indicates the ONU identifier string. 
             This attribute is relation to 'zxAnGponOPVoipIpMode'. 
             This attribute is valid only when 'zxAnGponOPVoipIpMode' is DHCP. 
             A unique ONU identifier string. If set to a non-null value,
             this string is used instead of the MAC address in retrieving
             DHCP parameters. If the string is shorter than 25 characters,
             it must be null terminated. Its default value is null bytes."
        REFERENCE
            "G.988 section 9.4.1 IP host config data"
        ::= { zxAnGponOPVoipIpConfEntry 7 }

    zxAnGponOPVoipIpAddrType     OBJECT-TYPE
        SYNTAX      InetAddressType
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "This attribute indicates the type of IP address denoted
             in zxAnGponOPVoipIpAddr object."
        DEFVAL { ipv4 }
        ::= { zxAnGponOPVoipIpConfEntry 8 }

    zxAnGponOPVoipIpAddr     OBJECT-TYPE
        SYNTAX      InetAddress
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "This attribute indicates the VoIP IP address.
             This attribute is relation to 'zxAnGponOPVoipIpMode'.
             This attribute is valid only when 'zxAnGponOPVoipIpMode'
             is static."
        ::= { zxAnGponOPVoipIpConfEntry 9 }

    zxAnGponOPVoipIpAddrPfxLen     OBJECT-TYPE
        SYNTAX      InetAddressPrefixLength
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "This attribute indicates the VoIP IP address prefix length.
             This attribute is only valid for IPv4 and this value is 0
             for IPv6."
        ::= { zxAnGponOPVoipIpConfEntry 10 }

    zxAnGponOPVoipIpPppoeMode OBJECT-TYPE
        SYNTAX      INTEGER{
            auto(1),
            chap(2),
            pap(3)
        }
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "This attribute indicates the VoIP IP PPPoE mode.
             This attribute is relation to 'zxAnGponOPVoipIpMode'.
             This attribute is valid only when 'zxAnGponOPVoipIpMode' is PPPoE."
        DEFVAL { auto }
        ::= { zxAnGponOPVoipIpConfEntry 11 }

    zxAnGponOPVoipIpPppoeUsername OBJECT-TYPE
        SYNTAX      DisplayString(SIZE(0..128))
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "This attribute indicates the VoIP IP PPPoE user name.
             This attribute is relation to 'zxAnGponOPVoipIpMode'.
             This attribute is valid only when 'zxAnGponOPVoipIpMode' is PPPoE."
        ::= { zxAnGponOPVoipIpConfEntry 12 }

    zxAnGponOPVoipIpPppoePassword OBJECT-TYPE
        SYNTAX      DisplayString(SIZE(0..25))
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "This attribute indicates the VoIP IP PPPoE password.
             This attribute is relation to 'zxAnGponOPVoipIpMode'.
             This attribute is valid only when 'zxAnGponOPVoipIpMode' is PPPoE."
        ::= { zxAnGponOPVoipIpConfEntry 13 }

    zxAnGponOPVoipIpRspPing OBJECT-TYPE
        SYNTAX      INTEGER{
            enabled(1),
            disabled(2)
        }
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "This attribute indicates whether respond to pings enabled."
        DEFVAL { enabled }
        ::= { zxAnGponOPVoipIpConfEntry 14 }

    zxAnGponOPVoipIpRspTraceRoute OBJECT-TYPE
        SYNTAX      INTEGER{
            enabled(1),
            disabled(2)
        }
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "This attribute indicates whether respond to trace route enabled.
             This attribute is only valid for IPv4 and this value is
             disabled(2) for IPv6."
        DEFVAL { disabled }
        ::= { zxAnGponOPVoipIpConfEntry 15 }
        
    zxAnGponOPVoipIpRsEnable OBJECT-TYPE
        SYNTAX      INTEGER{
            enabled(1),
            disabled(2)
        }
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION            
            "This attribute indicates enabling or disabling response to router 
             solicitation (RS) messages function. This attribute is only valid
             for IPv6 and this value is disabled(2) for IPv4."
        REFERENCE
             "G.988 section 9.4.5 IPv6 host config data."
        DEFVAL { disabled }
        ::= { zxAnGponOPVoipIpConfEntry 16 }
        
    zxAnGponOPVoipIpOnLinkPrefix OBJECT-TYPE
        SYNTAX      OCTET STRING (SIZE (17))
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "This attribute indicates the manually provisioned on-link prefix 
             used for destination IPv6 addresses of IPv6 host services. 
             The attribute is structured as follows: 
             Prefix length, number of leading bits in the prefix that are valid 
             (1 byte)+ prefix (16 bytes).
             This attribute is only valid for IPv6 and this value is 0x0 
             for IPv4."
        REFERENCE
            "G.988 section 9.4.5 IPv6 host config data"
        DEFVAL { '00'H  }            
        ::= { zxAnGponOPVoipIpConfEntry 17 } 

    zxAnGponOPVoipIpPppoeServiceName OBJECT-TYPE
        SYNTAX      DisplayString(SIZE(0..60))
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "This attribute indicates the PPPoE service name."
        ::= { zxAnGponOPVoipIpConfEntry 18 }

    zxAnGponOPVoipIpConfRowStatus OBJECT-TYPE
        SYNTAX      RowStatus
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
           "The status of this conceptual row.

           The supported actions of this conceptual row only include:

           To create a row in this table, a manager must
           set this object to createAndGo(4).

           To delete a row in this table, a manager must
           set this object to destroy(6).

           To query this object, always return 1. "
        ::= { zxAnGponOPVoipIpConfEntry 50 }

