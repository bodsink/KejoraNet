    zxAnGponOPExtVlanRuleConfTable     OBJECT-TYPE
        SYNTAX      SEQUENCE OF ZxAnGponOPExtVlanRuleConfEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The ONU profile extended VLAN rule configuration table."
        REFERENCE
            "G.988 9.3.13 Extended VLAN tagging operation configuration data"
        ::= { zxAnGponOnuServicePrfObjects 9 }

   zxAnGponOPExtVlanRuleConfEntry OBJECT-TYPE
        SYNTAX      ZxAnGponOPExtVlanRuleConfEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The ONU profile extended VLAN rule configuration entry."
        INDEX   { zxAnGponOPName, zxAnPonRmOnuIfIndex,
                 zxAnGponOPExtVlanConfFiltRule }
        ::= { zxAnGponOPExtVlanRuleConfTable 1 }

   ZxAnGponOPExtVlanRuleConfEntry ::=
        SEQUENCE {
            zxAnGponOPExtVlanConfFiltRule      DisplayString,
            zxAnGponOPExtVlanConfTreatRule     DisplayString,
            zxAnGponOPExtVlanConfRowStatus     RowStatus
        }

    zxAnGponOPExtVlanConfFiltRule OBJECT-TYPE
        SYNTAX      DisplayString(SIZE(1..64))
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This attribute refers to 'zxAnGponRmFiltRuleName'
             provides a unique profile of filter for this extended VLAN table."
        REFERENCE
            "G.988 9.3.13 Extended VLAN tagging operation configuration data"
        ::= { zxAnGponOPExtVlanRuleConfEntry 1 }

    zxAnGponOPExtVlanConfTreatRule OBJECT-TYPE
        SYNTAX      DisplayString(SIZE(1..64))
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "This attribute refers to 'zxAnGponRmTrtRuleName'
             provides a unique profile of treatment for this extended VLAN
             table."
        REFERENCE
            "G.988 9.3.13 Extended VLAN tagging operation configuration data"
        ::= { zxAnGponOPExtVlanRuleConfEntry 2 }

    zxAnGponOPExtVlanConfRowStatus OBJECT-TYPE
        SYNTAX      RowStatus
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "The status of this conceptual row.
             The state of this conceptual row only support
             'not exist' and  'active'.
             The action of this conceptual row only support
             'createAndGo' and 'destroy'.

             To create a row in this table, a manager must
             set this object to createAndGo(4).

             To delete a row in this table, a manager must
             set this object to destroy(6).

             To query this object, always return 1. "
        REFERENCE
            "G.988 9.3.13 Extended VLAN tagging operation configuration data"
        ::= { zxAnGponOPExtVlanRuleConfEntry 50 }
                
-------------------------------------------------------------------------------
--  zxAnGponOPBrgConfTable
-------------------------------------------------------------------------------
    zxAnGponOPBrgConfTable    OBJECT-TYPE
        SYNTAX      SEQUENCE OF ZxAnGponOPBrgConfEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The ONU profile bridge configuration table."
        REFERENCE
            "G.988 section 9.3.1 MAC Bridge service profile"
        ::= { zxAnGponOnuServicePrfObjects 10 }

    zxAnGponOPBrgConfEntry OBJECT-TYPE
        SYNTAX      ZxAnGponOPBrgConfEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The ONU profile bridge configuration entry."
        INDEX   { zxAnGponOPName, zxAnGponOPBrgIndex }
        ::= { zxAnGponOPBrgConfTable 1 }

    ZxAnGponOPBrgConfEntry ::=
        SEQUENCE {
            zxAnGponOPBrgIndex                 Integer32,
            zxAnGponOPBrgStpEnable             INTEGER,
            zxAnGponOPBrgLearningEnable        INTEGER,
            zxAnGponOPBrgLocalBridgeEnable     INTEGER,
            zxAnGponOPBrgPri                   Integer32,
            zxAnGponOPBrgMaxAge                Integer32,
            zxAnGponOPBrgHelloTime             Integer32,
            zxAnGponOPBrgForwardDelay          Integer32,
            zxAnGponOPBrgUnknownMacDiscard     INTEGER,
            zxAnGponOPBrgMacLearningDepth      Integer32,
            zxAnGponOPBrgAgingTime             Integer32
        }

    zxAnGponOPBrgIndex OBJECT-TYPE
        SYNTAX      Integer32
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This attribute indicates the index of MAC bridge in ONU, from 1 to
             65535. The first byte specifies the slot ID associated with this 
             MAC bridge, the second byte specifies the bridge group ID 
             associated with this MAC bridge."
        ::= { zxAnGponOPBrgConfEntry 1 }

    zxAnGponOPBrgStpEnable OBJECT-TYPE
        SYNTAX      INTEGER{
            enabled(1),
            disabled(2)
        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "This attribute indicates whether or not a spanning tree
             algorithm is enabled."
        REFERENCE
            "G.988 section 9.3.1 MAC Bridge service profile"
        ::= { zxAnGponOPBrgConfEntry 2 }

    zxAnGponOPBrgLearningEnable OBJECT-TYPE
        SYNTAX      INTEGER{
            enabled(1),
            disabled(2)
        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "This attribute indicates whether or not the learning
             function of the bridge is enabled."
        REFERENCE
            "G.988 section 9.3.1 MAC Bridge service profile"
        ::= { zxAnGponOPBrgConfEntry 3 }

    zxAnGponOPBrgLocalBridgeEnable OBJECT-TYPE
        SYNTAX      INTEGER{
            enabled(1),
            disabled(2)
        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "This attribute indicates whether or not the bridging between UNI
             ports is enabled."
        REFERENCE
            "G.988 section 9.3.1 MAC Bridge service profile"
        ::= { zxAnGponOPBrgConfEntry 4 }

    zxAnGponOPBrgPri OBJECT-TYPE
        SYNTAX      Integer32(0..65535)
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "This attribute specifies the bridge priority in the range 0..65535.
             The value of this attribute is copied to the bridge priority
             attribute of the associated MAC bridge configuration data managed
             entity."
        REFERENCE
            "G.988 section 9.3.1 MAC Bridge service profile"
        ::= { zxAnGponOPBrgConfEntry 5 }

    zxAnGponOPBrgMaxAge OBJECT-TYPE
        SYNTAX      Integer32(6..40)
        UNITS       "Seconds"
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "This attribute indicates the maximum age for an entry in the
             spanning tree listing. It indicates the maximum
             age in seconds for received protocol information before
             it is discarded."
        REFERENCE
            "G.988 section 9.3.1 MAC Bridge service profile"
        ::= { zxAnGponOPBrgConfEntry 6 }

    zxAnGponOPBrgHelloTime OBJECT-TYPE
        SYNTAX      Integer32(1..10)
        UNITS       "Seconds"
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "This attribute provides the time interval between hello packets.
             It is the time interval, in seconds, that a bridge advertises its
             presence while as a root or attempting to become a root."
        REFERENCE
            "G.988 section 9.3.1 MAC Bridge service profile"
        ::= { zxAnGponOPBrgConfEntry 7 }

    zxAnGponOPBrgForwardDelay OBJECT-TYPE
        SYNTAX      Integer32(4..30)
        UNITS       "Seconds"
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "This attribute gives the time that the
             bridge on the Ethernet card in the ONU retains a packet before
             forwarding it. (It indicates the value in seconds that
             the bridge uses for Forward Delay when the bridge acts as the
             root.) "
        REFERENCE
            "G.988 section 9.3.1 MAC Bridge service profile"
        ::= { zxAnGponOPBrgConfEntry 8 }

    zxAnGponOPBrgUnknownMacDiscard OBJECT-TYPE
        SYNTAX      INTEGER{
            enabled(1),
            disabled(2)
        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "This attribute indicates the treatment of MAC frames with
             unknown destination addresses.
             enabled: frames with unknown destination addresses will be
                     discarded.
             disabled: frames with unknown destination addresses will be
                     forwarded to all allowed ports."
        REFERENCE
            "G.988 section 9.3.1 MAC Bridge service profile"
        ::= { zxAnGponOPBrgConfEntry 9 }

    zxAnGponOPBrgMacLearningDepth OBJECT-TYPE
        SYNTAX      Integer32(0..255)
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "This attribute specifies the maximum number of UNI MAC addresses
             to be learned by the bridge.
             The default value 0 specifies that there is no
             administratively-imposed limit."
        REFERENCE
            "G.988 section 9.3.1 MAC Bridge service profile"
        ::= { zxAnGponOPBrgConfEntry 10 }

    zxAnGponOPBrgAgingTime OBJECT-TYPE
        SYNTAX      Integer32(10..1000000)
        UNITS       "Seconds"
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "This attribute specifies the age of dynamic filtering entries in
             the bridge database, after which unrefreshed entries are discarded.
             In accordance with [IEEE 802.1D] clause 7.9.2 and [IEEE 802.1Q]
             clause 8.8.3, the range is 10..1 000 000 seconds, with a resolution
             of 1 second and a default of 300 seconds."
        REFERENCE
            "G.988 section 9.3.1 MAC Bridge service profile"
        ::= { zxAnGponOPBrgConfEntry 11 }
        
