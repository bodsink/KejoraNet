ZTE-AN-PON-REMOTE-ONU-MIB DEFINITIONS ::= BEGIN
    IMPORTS 
        MODULE-IDENTITY, OBJECT-TYPE, Integer32, Unsigned32, Counter32,
        NOTIFICATION-TYPE, TimeTicks, IpAddress, 
        Integer32                                   FROM SNMPv2-SMI
        OBJECT-GROUP, MODULE-COMPLIANCE             FROM SNMPv2-CONF
        DisplayString, DateAndTime, MacAddress, TruthValue, RowStatus, 
        TEXTUAL-CONVENTION                          FROM SNMPv2-TC
        InetAddress, InetAddressType                FROM INET-ADDRESS-MIB
        ifIndex                                     FROM IF-MIB
        zxAnPon                                     FROM ZTE-AN-SMI
        ZxAnPonRmOnuIfIndex, ZxAnPonRmOnuUniIndex   FROM ZTE-AN-PON-TC-MIB
        zxAnPonOnuIndex                             FROM ZTE-AN-PON-BASE-MIB;
       
zxAnPonRemoteOnuMib MODULE-IDENTITY
    LAST-UPDATED    "201202270000Z"
    ORGANIZATION    "ZTE Corporation"
    CONTACT-INFO    "Guo HongJian      ZTE Corporation
                    Mail: guo.hongjian@zte.com.cn
                    Tel : +86-21-68897417"
    DESCRIPTION     
        "The MIB module for the management of PON remote ONU service."
        
    REVISION        "201202270000Z"
    DESCRIPTION
        "Initial version of this MIB module."
    ::= { zxAnPon 3 }

