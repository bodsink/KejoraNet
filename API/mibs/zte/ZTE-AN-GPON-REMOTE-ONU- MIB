    zxAnPonRmPotsCdrCalleeNumber OBJECT-TYPE
        SYNTAX      DisplayString
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Callee number."
        ::= { zxAnPonRmPotsCallDataRecordEntry 7 }  
        
    zxAnPonRmPotsCdrCallerNumber OBJECT-TYPE
        SYNTAX      DisplayString
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Caller number."
        ::= { zxAnPonRmPotsCallDataRecordEntry 8 }  
        
    zxAnPonRmPotsCdrTxRtpPkts OBJECT-TYPE
        SYNTAX      Integer32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The total number of 
             RTP packets transmitted during the call."
        ::= { zxAnPonRmPotsCallDataRecordEntry 9 }  
        
