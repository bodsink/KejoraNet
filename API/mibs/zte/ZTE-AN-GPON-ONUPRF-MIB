------------------------------------------------------------------------------
-- zxAnGponOPIpStaticRouteTable
------------------------------------------------------------------------------
    zxAnGponOPIpStaticRouteTable    OBJECT-TYPE
        SYNTAX      SEQUENCE OF ZxAnGponOPIpStaticRouteEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This table contains information of ONU IP static routes
             configuration. This managed entity stores IP static routes."
        REFERENCE   "G.984.4 section 9.4.5 IP static routes"
        ::= { zxAnGponOnuServicePrfObjects 15 }

    zxAnGponOPIpStaticRouteEntry OBJECT-TYPE
        SYNTAX      ZxAnGponOPIpStaticRouteEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "It is entry in the zxAnGponOPIpStaticRouteTable. The index
            'zxAnGponOPName' represents the normal ONU index."
        INDEX   { zxAnGponOPName, zxAnGponOPIpRouteDestType,
                  zxAnGponOPIpRouteDest, zxAnGponOPIpRoutePfxLen }
        ::= { zxAnGponOPIpStaticRouteTable 1 }

    ZxAnGponOPIpStaticRouteEntry ::=
        SEQUENCE {
            zxAnGponOPIpRouteDestType        InetAddressType,
            zxAnGponOPIpRouteDest            InetAddress,
            zxAnGponOPIpRoutePfxLen          InetAddressPrefixLength,
            zxAnGponOPIpRouteNextHopType     InetAddressType,
            zxAnGponOPIpRouteNextHop         InetAddress,
            zxAnGponOPIpRouteOutPort         Integer32,
            zxAnGponOPIpRouteRowStatus       RowStatus
        }

   zxAnGponOPIpRouteDestType OBJECT-TYPE
       SYNTAX     InetAddressType
       MAX-ACCESS not-accessible
       STATUS     current
       DESCRIPTION
           "The type of the 'zxAnGponOPIpRouteDest' address, as defined
            in the InetAddress MIB.
            Only those address types that may appear in an actual
            routing table are allowed as values of this object."
       REFERENCE
           "RFC 4001"
       ::= { zxAnGponOPIpStaticRouteEntry 1 }

    zxAnGponOPIpRouteDest OBJECT-TYPE
        SYNTAX      InetAddress
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The destination IP address of this route.
             The type of this address is determined by the value of
             the 'zxAnGponOPIpRouteDestType' object.
             The values for the index objects 'zxAnGponOPIpRouteDest' and
             'zxAnGponOPIpRoutePfxLen' must be consistent.  When the value
             of 'zxAnGponOPIpRouteDest' (excluding the zone index, if one
             is present) is x, then the bitwise logical-AND of x with the
             value of the mask formed from the corresponding index object
             'zxAnGponOPIpRoutePfxLen' must be equal to x. If not, then the
             index pair is not consistent and an inconsistentName error must
             be returned on SET or CREATE requests.
             This field may be set to the default route address  0.0.0.0 ."
        REFERENCE
            "G.984.4 section 9.4.5 IP static routes"
        ::= { zxAnGponOPIpStaticRouteEntry 2 }

    zxAnGponOPIpRoutePfxLen OBJECT-TYPE
        SYNTAX      InetAddressPrefixLength
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "Indicates the number of leading one bits that form the
             mask to be logical-ANDed with the destination address
             before being compared to the value in the
             zxAnGponOPIpRouteDest field.
             The values for the index objects 'zxAnGponOPIpRouteDest' and
             'zxAnGponOPIpRoutePfxLen' must be consistent.  When the value
             of 'zxAnGponOPIpRouteDest' (excluding the zone index, if one
             is present) is x, then the bitwise logical-AND of x with the
             value of the mask formed from the corresponding index object
             'zxAnGponOPIpRoutePfxLen' must be equal to x.  If not, then
             the index pair is not consistent and an inconsistentName error
             must be returned on SET or CREATE requests."
        REFERENCE
            "G.984.4 section 9.4.5 IP static routes"
        ::= { zxAnGponOPIpStaticRouteEntry 3 }

   zxAnGponOPIpRouteNextHopType OBJECT-TYPE
       SYNTAX     InetAddressType
       MAX-ACCESS read-create
       STATUS     current
       DESCRIPTION
           "The type of the 'zxAnGponOPIpRouteNextHopType' address, as
            defined in the InetAddress MIB.
            Value should be set to unknown(0) for non-remote  routes.
            Only those address types that may appear in an actual
            routing table are allowed as values of this object."
       REFERENCE
           "RFC 4001"
       ::= { zxAnGponOPIpStaticRouteEntry 4 }

    zxAnGponOPIpRouteNextHop OBJECT-TYPE
        SYNTAX      InetAddress
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "On remote routes, the address of the next system route.
             For non-remote routes, a zero length string.
             The type of this address is determined by the value of the
             'zxAnGponOPIpRouteNextHopType' object.
             This field is not used when the next hop is reached through an
             unnumbered link.
             For IPv4, the value is '255.255.255.255' when not used."
        REFERENCE
            "G.984.4 section 9.4.5 IP static routes"
        ::= { zxAnGponOPIpStaticRouteEntry 5 }

    zxAnGponOPIpRouteOutPort OBJECT-TYPE
        SYNTAX      Integer32(1..255)
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "The number of the IP port through which the next hop should be
             reached. This field is used when the next hop is reached through
             an unnumbered link and when a static route is set to support a
             fully meshed network. The value 0xFF indicates not used."
        REFERENCE
            "G.984.4 section 9.4.5 IP static routes"
        ::= { zxAnGponOPIpStaticRouteEntry 6 }

    zxAnGponOPIpRouteRowStatus  OBJECT-TYPE
        SYNTAX      RowStatus
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "The status of this conceptual row.
             To create a row in this table, a manager must set this object to
             createAndGo(4). In particular, a newly created row cannot be
             created until the corresponding attributes from
             'zxAnGponOPIpRouteNextHopType' to 'zxAnGponOPIpRouteOutPort'
             have been set. To delete a row in this table, a manager must
             set this object to destroy(6)."
        ::= { zxAnGponOPIpStaticRouteEntry 50 }

------------------------------------------------------------------------------
-- zxAnGponOPVeipTable
------------------------------------------------------------------------------
    zxAnGponOPVeipTable   OBJECT-TYPE
        SYNTAX      SEQUENCE OF ZxAnGponOPVeipEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "zxAnGponOPVeipTable table.
             This managed entity represents the data plane hand-off point in
             an ONU to a separate (non-OMCI) management domain.
             The virtual Ethernet interface point is managed by OMCI,
             and is potentially known to the non-OMCI management domain.
             One or more Ethernet traffic flows are present at this boundary."
        REFERENCE
            "G.988 session 9.5.5 virtual Ethernet interface point"
        ::= { zxAnGponOnuServicePrfObjects 16 }

    zxAnGponOPVeipEntry   OBJECT-TYPE
        SYNTAX      ZxAnGponOPVeipEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "GPON VEIP configuration entry. The index
            'zxAnGponOPName' represents the normal ONU index or the virtual
             ONU index for profile."
        INDEX   { zxAnGponOPName, zxAnGponOPVeipIndex }
        ::= { zxAnGponOPVeipTable 1 }

    ZxAnGponOPVeipEntry ::= SEQUENCE {
        zxAnGponOPVeipIndex                           Integer32,
        zxAnGponOPVeipAdminState                      INTEGER,
        zxAnGponOPVeipInterdomainName                 DisplayString,
        zxAnGponOPVeipIsPortAssigned                  INTEGER,
        zxAnGponOPVeipProtoType                       INTEGER,
        zxAnGponOPVeipAssignedPort                    Integer32,
        zxAnGponOPVeipHostId                          Integer32
        }

    zxAnGponOPVeipIndex  OBJECT-TYPE
        SYNTAX      Integer32(1..255)
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This attribute uniquely identifies each instance
             of this managed entity.
             To facilitate easy discovery,
             the ONU should assign IDs in the sequence 1, 2, ....
             The values 0 and 0xFFFF are reserved and not used now."
        REFERENCE
            "G.988 session 9.5.5 virtual Ethernet interface point"
        ::= { zxAnGponOPVeipEntry 1 }

    zxAnGponOPVeipAdminState    OBJECT-TYPE
        SYNTAX      INTEGER {
            unlocked(1),
            locked(2)
        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "This attribute locks and unlocks the functions performed by
             this managed entity."
        REFERENCE
            "G.988 session 9.5.5 virtual Ethernet interface point"
        DEFVAL  { unlocked }
        ::= {zxAnGponOPVeipEntry 2 }

    zxAnGponOPVeipInterdomainName    OBJECT-TYPE
        SYNTAX      DisplayString  ( SIZE ( 0 .. 25  ) )
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "This attribute is a character string that provides an optional way
             to identify the virtual Ethernet interface point
             to a non-OMCI management domain.
             The interface may also be identified by its managed entity ID,
             IANA assigned port and possibly other ways.
             If the vendor offers no information in this attribute,
             it should be set to a sequence of null bytes."
        REFERENCE
            "G.988 session 9.5.5 virtual Ethernet interface point"
        ::= { zxAnGponOPVeipEntry 3 }

    zxAnGponOPVeipIsPortAssigned OBJECT-TYPE
        SYNTAX      INTEGER{
            enabled(1),
            disabled(2)
        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "This attribute points to an instance of the TCP/UDP config data
             managed entity, which provides for OMCI management of
             the non-OMCI management domain's IP connectivity.
             This attribute specifies whether to assign tcp or udp port
             for IP connectivity."
        REFERENCE
            "G.988 session 9.5.5 virtual Ethernet interface point"
        DEFVAL     { enabled }
        ::= { zxAnGponOPVeipEntry 4 }

    zxAnGponOPVeipProtoType OBJECT-TYPE
        SYNTAX      INTEGER {
            tcp(1),
            udp(2)
        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "This attribute points to an instance of the TCP/UDP config data
             managed entity, which provides for OMCI management of
             the non-OMCI management domain's IP connectivity.
             This attribute specifies the protocol used in IP connectivity."
        REFERENCE
            "G.988 session 9.5.5 virtual Ethernet interface point"
        DEFVAL  { udp }
        ::= {zxAnGponOPVeipEntry 5 }

    zxAnGponOPVeipAssignedPort  OBJECT-TYPE
        SYNTAX      Integer32(0..65535)
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "This attribute points to an instance of the TCP/UDP config data
             managed entity, which provides for OMCI management of
             the non-OMCI management domain's IP connectivity.
             This attribute specifies the assigned tcp or udp port
             for IP connectivity when zxAnGponOPVeipIsTcpOrUdpPortAssigned is
             true."
        REFERENCE
            "G.988 session 9.5.5 virtual Ethernet interface point"
        ::= { zxAnGponOPVeipEntry 6 }

    zxAnGponOPVeipHostId  OBJECT-TYPE
        SYNTAX      Integer32(1..255)
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "This attribute points to an instance of the TCP/UDP config data
             managed entity, which provides for OMCI management of
             the non-OMCI management domain's IP connectivity
             when zxAnGponOPVeipIsTcpOrUdpPortAssigned is true.
             This attribute specifies the host ID for IP connectivity."
        REFERENCE
            "G.988 session 9.5.5 virtual Ethernet interface point"
        ::= { zxAnGponOPVeipEntry 7 }

------------------------------------------------------------------------------
-- zxAnGponOPTr069AcsTable
------------------------------------------------------------------------------
    zxAnGponOPTr069AcsTable   OBJECT-TYPE
        SYNTAX      SEQUENCE OF ZxAnGponOPTr069AcsEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "zxAnGponOPTr069AcsTable table.
             If functions within the ONU are managed by [BBF TR-69],
             this ME allows OMCI configuration of the ACS URL
             and related authentication information for an ACS connection
             initiated by the ONU.
             TR-69 supports other means to discover its ACS,
             so not all TR-69-compatible ONUs necessarily support this ME.
             Further, even if the ONU does support this ME,
             some operators may choose not to use it."
        REFERENCE
            "G.988 session 9.12.16 TR-069 management server"
        ::= { zxAnGponOnuServicePrfObjects 17 }

    zxAnGponOPTr069AcsEntry   OBJECT-TYPE
        SYNTAX      ZxAnGponOPTr069AcsEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "GPON Tr069 management server configuration entry. The index
            'zxAnGponOPName' represents the normal ONU index or the virtual
             ONU index for profile."
        INDEX   { zxAnGponOPName, zxAnGponOPVeipIndex }
        ::= { zxAnGponOPTr069AcsTable 1 }

    ZxAnGponOPTr069AcsEntry ::= SEQUENCE {
        zxAnGponOPTr069AcsAdminState               INTEGER,
        zxAnGponOPTr069AcsUri                      DisplayString,
        zxAnGponOPTr069AcsValidation               INTEGER,
        zxAnGponOPTr069AcsUsername                 DisplayString,
        zxAnGponOPTr069AcsPassword                 DisplayString,
        zxAnGponOPTr069AcsRealm                    DisplayString,
        zxAnGponOPTr069AssociatedTag               Integer32
        }

    zxAnGponOPTr069AcsAdminState    OBJECT-TYPE
        SYNTAX      INTEGER {
            unlocked(1),
            locked(2)
        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "This attribute locks and unlocks the functions performed
             by this managed entity. When the administrative state is locked,
             the functions of this ME are disabled.
             TR-69 connectivity to an ACS may be possible through means
             that do not depend on this ME.
             The default value of this attribute is locked."
        REFERENCE
            "G.988 session 9.12.16 TR-069 management server"
        DEFVAL  { locked }
        ::= {zxAnGponOPTr069AcsEntry 1 }

    zxAnGponOPTr069AcsUri OBJECT-TYPE
        SYNTAX       DisplayString  ( SIZE ( 0 .. 128  ) )
        MAX-ACCESS   read-write
        STATUS       current
        DESCRIPTION
            "This attribute points to the large string ME that contains
             the network address.
             It may contain a fully qualified domain name,
             URI or IP address. The URI may also contain a port identifier,
             eg x.y.z.com:5060.
             A null pointer indicates that no network address is defined."
        REFERENCE
            "G.988 session 9.12.3 Network address"
        ::= { zxAnGponOPTr069AcsEntry 2 }

    zxAnGponOPTr069AcsValidation    OBJECT-TYPE
        SYNTAX      INTEGER {
            disabled(1),
            md5(2),
            basic(3)
        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "This attribute specifies the validation scheme used when the ONU
             validates a challenge. Validation schemes are defined as follows:
             1  Validation disabled.
             2  Validate using MD5 digest authentication as defined in
                [IETF RFC 2069].
             3  Validate using basic authentication as defined in
                [IETF RFC 2617]."
        REFERENCE
            "G.988 section 9.12.4 Authentication security method"
        DEFVAL  { disabled }
        ::= {zxAnGponOPTr069AcsEntry 3 }

    zxAnGponOPTr069AcsUsername    OBJECT-TYPE
        SYNTAX      DisplayString  ( SIZE ( 0 .. 5  ) )
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "This string attribute is the user name. If the string is shorter
             than 25 bytes, it must be null terminated."
        REFERENCE
            "G.988 section 9.12.4 Authentication security method"
        ::= {zxAnGponOPTr069AcsEntry 4 }

    zxAnGponOPTr069AcsPassword    OBJECT-TYPE
        SYNTAX      DisplayString  ( SIZE ( 0 .. 25  ) )
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "This string attribute is the password. If the string is shorter
             than 25 bytes, it must be null terminated."
        REFERENCE
            "G.988 section 9.12.4 Authentication security method"
        ::= {zxAnGponOPTr069AcsEntry 5 }

    zxAnGponOPTr069AcsRealm    OBJECT-TYPE
        SYNTAX      DisplayString  ( SIZE ( 0 .. 25  ) )
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "This string attribute specifies the realm used in md5 digest
             authentication. If the string is shorter than 25 bytes,
             it must be null terminated."
        REFERENCE
            "G.988 section 9.12.4 Authentication security method"
        ::= {zxAnGponOPTr069AcsEntry 6 }

    zxAnGponOPTr069AssociatedTag    OBJECT-TYPE
        SYNTAX      Integer32(0..65535)
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "This attribute is a TCI value for TR-69 management traffic
             passing through the virtual ethernet interface point.
             A TCI, comprising user priority, CFI and VID,
             is represented by 2 bytes.
             the first 3 bits represent priority.
             the last 12 bits represent VID.
             eg: priority(3 bits)| reserved(1 bits) |VID(12bits).
             The value 0xFFFF specifies that TR-69 management traffic
             passes through the virtual ethernet interface point
             with neither a VLAN nor a priority tag."
        REFERENCE
            "G.988 session 9.12.16 TR-069 management server"
        ::= {zxAnGponOPTr069AcsEntry 7 }
        
-------------------------------------------------------------------------------
-- zxAnGponOPEthUniTable
-------------------------------------------------------------------------------
    zxAnGponOPEthUniTable    OBJECT-TYPE
        SYNTAX      SEQUENCE OF ZxAnGponOPEthUniEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The ONU profile Ethernet UNI configuration table."
        REFERENCE
            "G.988 section 9.5.1 Physical path termination point
             Ethernet UNI"
        ::= { zxAnGponOnuServicePrfObjects 18 }

    zxAnGponOPEthUniEntry OBJECT-TYPE
        SYNTAX      ZxAnGponOPEthUniEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "The ONU profile Ethernet UNI configuration entry."            
        INDEX   { zxAnGponOPName, zxAnPonRmOnuUniIndex }
        ::= { zxAnGponOPEthUniTable 1 }

    ZxAnGponOPEthUniEntry ::=
        SEQUENCE {
            zxAnGponOPEthUniExpectedType            Integer32,
            zxAnGponOPEthUniIfSpeedConf             INTEGER,
            zxAnGponOPEthUniLoopbackConf            INTEGER,
            zxAnGponOPEthUniAdminState              INTEGER,
            zxAnGponOPEthUniMaxFrameSize            Integer32,
            zxAnGponOPEthUniDteOrDceInd             INTEGER,
            zxAnGponOPEthUniPauseTime               Integer32,
            zxAnGponOPEthUniBridgeOrIpInd           INTEGER,
            zxAnGponOPEthUniArcEnable               INTEGER,
            zxAnGponOPEthUniArcInterval             Integer32,
            zxAnGponOPEthUniPppoeFilter             INTEGER,
            zxAnGponOPEthUniPowerFeed               INTEGER,
            zxAnGponOPEthUniLoopDetect              INTEGER
        }

    zxAnGponOPEthUniExpectedType OBJECT-TYPE
        SYNTAX      Integer32(0..255)
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The following coding is used for this attribute:
             0x00 = by autosensing, 0x01 to 0xFE (1 to 254) = one of the values
             from table 3 that is compatible with the type of the LIM. Upon
             autonomous instantiation, the value 0x00 is used."
        REFERENCE
            "G.988 section 9.5.1 Physical path termination point Ethernet
             UNI"
        ::= { zxAnGponOPEthUniEntry 1 }

    zxAnGponOPEthUniIfSpeedConf OBJECT-TYPE
        SYNTAX      INTEGER{
            auto(1),
            half10(2),
            full10(3),
            half100(4),
            full100(5),
            full1000(6),
            full10000(7)
        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "For 10/100 Base-Tx Ethernet interfaces, this attribute is used to
             set the configuration options:
             auto(1): Auto-sensing;
             half10(2): 10BaseT (half duplex);
             full10(3): 10BaseT (full duplex);
             half100(4): 100BaseT (half duplex);
             full100(5): 100BaseT (full duplex);
             full1000(6): Gigabit Ethernet (full duplex);
             full10000(7): 10G Ethernet (full duplex);"
        REFERENCE
            "G.988 section 9.5.1 Physical path termination point
             Ethernet UNI"
        ::= { zxAnGponOPEthUniEntry 2 }

    zxAnGponOPEthUniLoopbackConf OBJECT-TYPE
        SYNTAX      INTEGER{
            noLoop(1),
            loop3(4)
        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "This attribute is used to set the Ethernet loopback configuration:
             No loopback (value 0x01), loop3 (value 0x04, Loopback of downstream
             traffic after PHY transceiver)."
        REFERENCE
            "G.988 section 9.5.1 Physical path termination point
             Ethernet UNI"
        ::= { zxAnGponOPEthUniEntry 3 }

    zxAnGponOPEthUniAdminState OBJECT-TYPE
        SYNTAX      INTEGER{
            unlocked(1),
            locked(2)
        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "This attribute is used to activate (unlock: value 0x01) and
             deactivate(lock: value 0x02) the functions performed by instances
             of this managed entity."
        REFERENCE
            "G.988 section 9.5.1 Physical path termination point
             Ethernet UNI"
        ::= { zxAnGponOPEthUniEntry 4 }
        
    zxAnGponOPEthUniMaxFrameSize OBJECT-TYPE
        SYNTAX      Integer32(64..12288)
        UNITS       "bytes"
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "This attribute denotes the maximum allowed frame size to be
             transmitted across this interface."
        REFERENCE
            "G.988 section 9.5.1 Physical path termination point
             Ethernet UNI"
        ::= { zxAnGponOPEthUniEntry 5 }

    zxAnGponOPEthUniDteOrDceInd OBJECT-TYPE
        SYNTAX      INTEGER{
            dce(1),
            dte(2),
            auto(3)
        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "This attribute specifies the Ethernet interface wiring:
             1 DCE or MDI-X (default).
             2 DTE or MDI.
             3 Automatic selection"
        REFERENCE
            "G.988 section 9.5.1 Physical path termination point
             Ethernet UNI"
        DEFVAL { dce }
        ::= { zxAnGponOPEthUniEntry 6 }

    zxAnGponOPEthUniPauseTime OBJECT-TYPE
        SYNTAX      Integer32(0..65535)
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "This attribute allows the ONU line card to request that the
             customer terminal temporarily suspend sending data. Units are in
             pause quanta(1 pause_quantum equals 512 bits times of the
             particular implementation)."
        REFERENCE
            "G.988 section 9.5.1 Physical path termination point
             Ethernet UNI"
        ::= { zxAnGponOPEthUniEntry 7 }

    zxAnGponOPEthUniBridgeOrIpInd OBJECT-TYPE
        SYNTAX      INTEGER{
            bridged(1),
            ipRouter(2),
            either(3)
        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "This attribute specifies whether the Ethernet interface is bridged
             or derived from an IP router function.
             1 Bridged
             2 IP router
             3 Depends on the parent circuit pack.

             3 means that the circuit pack's bridged or IP ind attribute is
             either 1 or 2."
        REFERENCE
            "G.988 section 9.5.1 Physical path termination point
             Ethernet UNI"
        ::= { zxAnGponOPEthUniEntry 8 }

    zxAnGponOPEthUniArcEnable OBJECT-TYPE
        SYNTAX      INTEGER{
            enabled(1),
            disabled(2)
        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "This attribute allows the activation of alarm reporting control
             (ARC) for this PPTP or cardholder. The attribute works in concert
             with the ARC interval attribute.
             enabled: enables ARC.
             disabled: disables ARC."
        REFERENCE
            "G.988 section 9.5.1 Physical path termination point
             Ethernet UNI"
        ::= { zxAnGponOPEthUniEntry 9 }

    zxAnGponOPEthUniArcInterval OBJECT-TYPE
        SYNTAX      Integer32(0..255)
        UNITS       "Minutes"
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "This attribute defines the interval to be used with the ARC
             function for this PPTP or cardholder. The values 0 through 254
             give the duration in minutes for the NALM-QI timer of M.3100.
             The special value 255 means that the timer never expires.
             The default value is zero."
        REFERENCE
            "G.988 section 9.5.1 Physical path termination point
             Ethernet UNI"
        ::= { zxAnGponOPEthUniEntry 10 }

    zxAnGponOPEthUniPppoeFilter OBJECT-TYPE
        SYNTAX      INTEGER{
            enabled(1),
            disabled(2)
        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "This attribute may be used to control filtering of PPPoE packets
             on this Ethernet port. The value 2 is used to disable filtering.
             The value 1 is used to enable filtering. When filtering is
             enabled, all packets other than PPPoE packets will be discarded."
        REFERENCE
            "G.988 section 9.5.1 Physical path termination point
             Ethernet UNI"
        ::= { zxAnGponOPEthUniEntry 11 }

    zxAnGponOPEthUniPowerFeed OBJECT-TYPE
        SYNTAX      INTEGER{
            enabled(1),
            disabled(2)
        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "This attribute controls whether power is provided to an external
             equipment over the Ethernet PPTP. Value 1 enables power over
             Ethernet port. The default value 2 disables power feed."
        REFERENCE
            "G.988 section 9.5.1 Physical path termination point
             Ethernet UNI"
        ::= { zxAnGponOPEthUniEntry 12 }

    zxAnGponOPEthUniLoopDetect OBJECT-TYPE
        SYNTAX      INTEGER{
            enabled(1),
            disabled(2)
        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "This attribute enable or disable the loopback detection."
        REFERENCE
            "G.988 section 9.5.1 Physical path termination point
             Ethernet UNI"
        ::= { zxAnGponOPEthUniEntry 13 }

